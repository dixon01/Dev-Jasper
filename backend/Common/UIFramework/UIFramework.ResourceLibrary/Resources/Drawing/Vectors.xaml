                                       Width="28.5"
                                       Height="28.5"
                                       Fill="White"
                                       Stretch="Fill">
                                <Rectangle.Effect>
                                    <DropShadowEffect ShadowDepth="1" />
                                </Rectangle.Effect>
                            </Rectangle>
                            <Rectangle Width="100"
