<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    mc:Ignorable="d">
    <Style x:Key="DeleteIcon" TargetType="{x:Type ContentControl}">
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Viewbox Width="16" Height="16">
                        <Grid Width="100" Height="100">
                            <Ellipse Width="100"
                                     Height="100"
                                     HorizontalAlignment="Left"
                                     VerticalAlignment="Top"
                                     Fill="Black"
                                     Stroke="Black" />
                            <Path Margin="20"
                                  Data="M0,0 L100,100"
                                  Fill="White"
                                  Stretch="Fill"
                                  Stroke="White"
                                  StrokeThickness="15"
                                  UseLayoutRounding="False" />
                            <Path Margin="20"
                                  Data="M100,0 L0,100"
                                  Fill="White"
                                  Stretch="Fill"
                                  Stroke="White"
                                  StrokeThickness="15"
                                  UseLayoutRounding="False" />
                        </Grid>
                    </Viewbox>
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="WindowEffectNone" TargetType="ContentControl">
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Viewbox Width="16" Height="16">
                        <Grid Width="100"
                              Height="100"
                              ToolTipService.ToolTip="No Effect">
                            <Rectangle Margin="1"
                                       Fill="White"
                                       StrokeThickness="10">
                                <Rectangle.Stroke>
                                    <LinearGradientBrush MappingMode="RelativeToBoundingBox" StartPoint="0.5,0" EndPoint="0.5,1">
                                        <GradientStop Offset="0" Color="#FF454545" />
                                        <GradientStop Offset="1" Color="#FF727272" />
                                    </LinearGradientBrush>
                                </Rectangle.Stroke>
                            </Rectangle>
                        </Grid>
                    </Viewbox>
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="UpNavigate" TargetType="ContentControl">
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Viewbox Width="16" Height="16">
                        <Grid Width="100"
                              Height="100"
                              ToolTipService.ToolTip="No Effect">
                            <Path Canvas.Left="30.0833"
                                  Canvas.Top="22.1667"
                                  Width="70"
                                  Height="90"
                                  Data="F1 M 30.0833,22.1667L 50.6665,37.6043L 50.6665,38.7918L 30.0833,53.8333L 30.0833,22.1667 Z "
                                  Fill="White"
                                  RenderTransformOrigin="0.5,0.5"
                                  Stretch="Fill">
                                <Path.Effect>
                                    <DropShadowEffect ShadowDepth="1" />
                                </Path.Effect>
                                <Path.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform />
                                        <SkewTransform />
                                        <RotateTransform Angle="-90" />
                                        <TranslateTransform />
                                    </TransformGroup>
                                </Path.RenderTransform>
                            </Path>
                        </Grid>
                    </Viewbox>
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="PlayIcon" TargetType="ContentControl">
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Viewbox Width="32" Height="32">
                        <Grid Width="100"
                              Height="100"
                              ToolTipService.ToolTip="Play">
                            <Path Canvas.Left="30.0833"
                                  Canvas.Top="22.1667"
                                  Width="20.5832"
                                  Height="31.6667"
                                  Data="F1 M 30.0833,22.1667L 50.6665,37.6043L 50.6665,38.7918L 30.0833,53.8333L 30.0833,22.1667 Z "
                                  Fill="White"
                                  Stretch="Fill">
                                <Path.Effect>
                                    <DropShadowEffect ShadowDepth="1" />
                                </Path.Effect>
                            </Path>
                            <Rectangle Width="100"
                                       Height="100"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Top"
                                       Fill="White"
                                       Opacity="0.25" />
                        </Grid>
                    </Viewbox>
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="StopIcon" TargetType="ContentControl">
