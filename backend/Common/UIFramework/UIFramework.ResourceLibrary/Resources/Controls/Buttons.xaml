                                Grid.RowSpan="3"
                                Grid.ColumnSpan="3"
                                BorderBrush="#7F000000">
                            <Border.Background>
                                <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                                    <GradientStop Color="#3FFFFFFF" />
                                    <GradientStop Offset="1" Color="#A0000000" />
                                    <GradientStop Offset="0.518" Color="#008A8A8A" />
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                        <Border Grid.RowSpan="3" Grid.ColumnSpan="3">
                            <Border.Background>
                                <RadialGradientBrush GradientOrigin="0.494,2.489" RadiusX="0.689" RadiusY="0.689">
                                    <GradientStop Offset="0.856" Color="#99000000" />
                                    <GradientStop Offset="0" Color="#66FFFFFF" />
                                </RadialGradientBrush>
                            </Border.Background>
                        </Border>
                        <Border x:Name="ellipse"
                                Grid.Row="2"
                                Grid.Column="0"
                                Grid.ColumnSpan="3"
                                Margin="0,-1,0,0"
                                Opacity="0.145">
                            <Border.Background>
                                <RadialGradientBrush RadiusX="0.469" RadiusY="0.807">
                                    <RadialGradientBrush.RelativeTransform>
                                        <TransformGroup>
                                            <ScaleTransform CenterX="0.5"
                                                            CenterY="0.5"
                                                            ScaleX="1.48"
                                                            ScaleY="1.48" />
                                            <SkewTransform CenterX="0.5" CenterY="0.5" />
                                            <RotateTransform CenterX="0.5" CenterY="0.5" />
