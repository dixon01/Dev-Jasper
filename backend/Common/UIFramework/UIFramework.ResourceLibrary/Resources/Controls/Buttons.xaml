                                    </RadialGradientBrush.RelativeTransform>
                                    <GradientStop Color="#BFFF2F00" />
                                    <GradientStop Offset="1" Color="Transparent" />
                                </RadialGradientBrush>
                            </Border.Background>
                        </Border>
                        <ContentPresenter x:Name="contentPresenter"
                                          Grid.Row="1"
                                          Grid.RowSpan="1"
                                          Grid.Column="1"
                                          HorizontalAlignment="Center"
                                          VerticalAlignment="Center"
                                          Content="Hello" />
