                            <Path x:Name="path" Data="M8.6794548,5.7519989 L11.402781,9.753685 L8.6149979,13.749999 L9.7752314,13.749999 L12.009755,10.457336 L14.190565,13.749999 L15.420627,13.749999 L12.665072,9.7053423 L15.425999,5.7519989 L14.260393,5.7519989 L12.058099,9.0016899 L9.9041462,5.7519989 z M12.5,0.5 C19.127417,0.49999988 24.5,4.7532949 24.5,10 C24.5,15.246705 19.127417,19.5 12.5,19.5 C5.8725829,19.5 0.5,15.246705 0.5,10 C0.5,4.7532949 5.8725829,0.49999988 12.5,0.5 z"
                            Fill="#FF83B9BD" Stretch="Fill" Stroke="Transparent" UseLayoutRounding="False" Margin="0,0,0,1.5" HorizontalAlignment="Right" VerticalAlignment="Bottom" Opacity="0" Visibility="Visible" Width="18" Height="17.5"/>
