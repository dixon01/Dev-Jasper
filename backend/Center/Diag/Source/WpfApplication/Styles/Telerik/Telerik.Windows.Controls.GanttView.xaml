                    <Grid x:Name="TemplateRootVisual" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="20"/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <Border x:Name="PART_ExpandCollapseHitArea" MinHeight="15" Background="Transparent" Grid.Column="0" HorizontalAlignment="Stretch">
                            <Path x:Name="PART_ExpandCollapseVisual"
                                    StrokeThickness="1"
                                    RenderTransformOrigin="0.5,0.5"
                                    Stretch="Fill"
                                    Data="M0,0 L4,3 L0,6 z"
                                    Width="4"
                                    Height="6"
                                    VerticalAlignment="Stretch"
                                    HorizontalAlignment="Stretch"
                                    IsHitTestVisible="False"
                                    Margin="-4 0 0 0"/>
                        </Border>
                        <ContentPresenter
                                Grid.Column="1"
                                Margin="{TemplateBinding Padding}"
                                HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="gantt:TreeCellContainer.ExpandCollapseVisualCollapsedStyle">
            <Setter.Value>
                <Style TargetType="Path">
                    <Setter Property="Fill" Value="{telerik:Windows8Resource ResourceKey=AccentBrush}"/>
                </Style>
            </Setter.Value>
        </Setter>
        <Setter Property="gantt:TreeCellContainer.ExpandCollapseVisualExpandedStyle">
