                <Border
                        Padding="{TemplateBinding BorderThickness}"
                        BorderBrush="{StaticResource DragandDropBorderBrush}"
                        BorderThickness="{TemplateBinding BorderThickness}"
                        CornerRadius="{StaticResource GanttDragVisualCue_CornerRadius}"
                        Background="{TemplateBinding Background}"
                        Opacity="0.9"/>
