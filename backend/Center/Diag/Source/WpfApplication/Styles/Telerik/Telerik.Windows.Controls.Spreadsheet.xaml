                                    <telerik:RadListBox.ItemTemplate>
                                        <DataTemplate>
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition/>
                                                    <ColumnDefinition/>
                                                    <ColumnDefinition/>
                                                </Grid.ColumnDefinitions>
                                                <Grid Grid.Column="0">
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="Auto"/>
                                                        <ColumnDefinition Width="*"/>
                                                    </Grid.ColumnDefinitions>
                                                    <TextBlock Text="{Binding Path=Header}" Width="50" Grid.Column="0" VerticalAlignment="Center"/>
                                                    <telerik:RadComboBox
                                                            ItemsSource="{Binding Path=SortByItemsSource}"
                                                            SelectedItem="{Binding Path=SelectedRelativeIndex, Mode=TwoWay}"
                                                            HorizontalAlignment="Stretch"
                                                            Grid.Column="1"
                                                            Margin="0 0 10 0">
                                                        <telerik:RadComboBox.ItemTemplate>
                                                            <DataTemplate>
                                                                <ContentPresenter Content="{Binding Path=., Converter={StaticResource IntToColumnNameConverter}}"/>
                                                            </DataTemplate>
                                                        </telerik:RadComboBox.ItemTemplate>
                                                    </telerik:RadComboBox>
                                                </Grid>
                                                <telerik:RadComboBox
                                                        Grid.Column="1"
                                                        HorizontalAlignment="Stretch"
                                                        SelectedItem="{Binding Path=SelectedSortType, Mode=TwoWay}"
                                                        ItemsSource="{Binding Path=SortTypesItemsSource}"
                                                        Margin="0 0 10 0">
                                                    <telerik:RadComboBox.ItemTemplate>
                                                        <DataTemplate>
                                                            <ContentPresenter Content="{Binding Path=., Converter={StaticResource SortTypeToStringConverter}}"/>
                                                        </DataTemplate>
                                                    </telerik:RadComboBox.ItemTemplate>
                                                </telerik:RadComboBox>
                                                <Grid Grid.Column="2" Visibility="{Binding Path=IsColorsSortOrderVisible, Converter={StaticResource BoolToVisibilityValueConverter}}">
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="*"/>
                                                        <ColumnDefinition Width="*"/>
                                                    </Grid.ColumnDefinitions>
                                                    <telerik:RadComboBox
                                                            HorizontalAlignment="Stretch"
                                                            x:Name="ColorSelector"
                                                            Grid.Column="0"
                                                            Width="88"
                                                            SelectedIndex="{Binding Path=SelectedFillIndex, Mode=TwoWay}"
                                                            ItemsSource="{Binding Path=FillsItemsSource}"
                                                            Margin="0 0 10 0">
                                                        <telerik:RadComboBox.ItemTemplateSelector>
                                                            <spreadsheetUtilities:PatternGradientFillTemplateSelector>
                                                                <spreadsheetUtilities:PatternGradientFillTemplateSelector.DefaultPatternFillTemplate>
                                                                    <DataTemplate>
                                                                        <TextBlock Text="No Fill" Width="80" Height="18"/>
                                                                    </DataTemplate>
                                                                </spreadsheetUtilities:PatternGradientFillTemplateSelector.DefaultPatternFillTemplate>
                                                                <spreadsheetUtilities:PatternGradientFillTemplateSelector.PatternFillTemplate>
                                                                    <DataTemplate>
                                                                        <spreadsheetControls:PatternFillControl
                                                                                Width="80"
                                                                                Height="18"
                                                                                ColorScheme="{Binding ElementName=ColorSelector, Path=DataContext.ColorPalette.ColorScheme}"
                                                                                PatternType="{Binding Path=PatternType}"
                                                                                PatternColor="{Binding Path=., Converter={StaticResource PatternFillToPatternColorConverter}}"
                                                                                BackgroundColor="{Binding Path=., Converter={StaticResource PatternFillToBackgroundColorConverter}}"/>
                                                                    </DataTemplate>
                                                                </spreadsheetUtilities:PatternGradientFillTemplateSelector.PatternFillTemplate>
                                                                <spreadsheetUtilities:PatternGradientFillTemplateSelector.GradientFillTemplate>
                                                                    <DataTemplate>
                                                                        <spreadsheetControls:GradientFillControl
                                                                                MaxWidth="80"
                                                                                MaxHeight="18"
                                                                                ColorScheme="{Binding ElementName=ColorSelector, Path=DataContext.ColorPalette.ColorScheme}"
                                                                                GradientType="{Binding Path=GradientType}"
                                                                                Color1="{Binding Path=Color1}"
                                                                                Color2="{Binding Path=Color2}"/>
                                                                    </DataTemplate>
                                                                </spreadsheetUtilities:PatternGradientFillTemplateSelector.GradientFillTemplate>
                                                            </spreadsheetUtilities:PatternGradientFillTemplateSelector>
                                                        </telerik:RadComboBox.ItemTemplateSelector>
                                                    </telerik:RadComboBox>
                                                    <telerik:RadComboBox HorizontalAlignment="Stretch" Grid.Column="1" ItemsSource="{Binding Path=SortOrderItemsSource}" SelectedIndex="{Binding Path=SelectedSortOrderIndex, Mode=TwoWay}"/>
                                                </Grid>
                                                <telerik:RadComboBox
                                                        HorizontalAlignment="Stretch"
                                                        MaxWidth="185"
                                                        Visibility="{Binding Path=IsColorsSortOrderVisible, Converter={StaticResource InvertedBooleanToVisibilityConverter}}"
                                                        Grid.Column="2"
                                                        ItemsSource="{Binding Path=SortOrderItemsSource}"
                                                        SelectedIndex="{Binding Path=SelectedSortOrderIndex, Mode=TwoWay}"
                                                        ScrollViewer.HorizontalScrollBarVisibility="Hidden">
                                                    <telerik:RadComboBox.ItemsPanel>
                                                        <ItemsPanelTemplate>
                                                            <StackPanel MaxWidth="300"/>
                                                        </ItemsPanelTemplate>
                                                    </telerik:RadComboBox.ItemsPanel>
                                                </telerik:RadComboBox>
                                            </Grid>
                                        </DataTemplate>
                                    </telerik:RadListBox.ItemTemplate>
                                </telerik:RadListBox>
                            </Grid>
                        </Border>
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Grid.Row="2" Margin="0 5 0 0">
                            <telerik:RadButton telerik:LocalizationManager.ResourceKey="Ok" Command="{Binding Path=OKCommand}" Margin="10 0 0 0" MinWidth="75"/>
                            <telerik:RadButton telerik:LocalizationManager.ResourceKey="Cancel" Command="{Binding Path=CancelCommand}" Margin="10 0 0 0" MinWidth="75"/>
                        </StackPanel>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="CustomSortListsDialogContentStyle" TargetType="dialogs:CustomSortListsDialogContent">
        <Setter Property="telerik:LocalizationManager.ResourceKey" Value="Spreadsheet_CustomSortListsDialog_Header"/>
        <Setter Property="Background" Value="{telerik:Windows8Resource ResourceKey=MainBrush}"/>
        <Setter Property="Padding" Value="5"/>
        <Setter Property="ResizeMode" Value="NoResize"/>
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
        <Setter Property="VerticalContentAlignment" Value="Stretch"/>
        <Setter Property="MinWidth" Value="600"/>
        <Setter Property="MinHeight" Value="360"/>
        <Setter Property="FontFamily" Value="{telerik:Windows8Resource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik:Windows8Resource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik:Windows8Resource ResourceKey=MarkerBrush}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="dialogs:CustomSortListsDialogContent">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <telerik:RadTabControl Margin="-2 0">
                            <telerik:RadTabItem telerik:LocalizationManager.ResourceKey="Spreadsheet_CustomSortListsDialog_CustomListsHeader">
                                <Grid Margin="10 5">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="190"/>
                                        <RowDefinition Height="*"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="240"/>
                                        <ColumnDefinition Width="240"/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_CustomSortListsDialog_CustomListsLabel"/>
                                    <telerik:RadListBox
                                            Grid.Row="1"
                                            Grid.Column="0"
                                            ScrollViewer.HorizontalScrollBarVisibility="Hidden"
                                            ScrollViewer.VerticalScrollBarVisibility="Visible"
                                            SelectedIndex="{Binding Path=SelectedCustomListIndex, Mode=TwoWay}"
                                            ItemsSource="{Binding Path=CustomListsItemsSource}"/>
                                    <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_CustomSortListsDialog_ListEntriesLabel" Margin="10 0 0 0" Grid.Row="0" Grid.Column="1"/>
                                    <TextBox
                                            Margin="10 0 0 0"
                                            AcceptsReturn="True"
                                            IsEnabled="{Binding Path=IsEnabled}"
                                            Grid.Row="1"
                                            Grid.Column="1"
                                            VerticalContentAlignment="Top"
                                            ScrollViewer.HorizontalScrollBarVisibility="Hidden"
                                            ScrollViewer.VerticalScrollBarVisibility="Visible"
                                            Text="{Binding Path=ListItems, Mode=TwoWay}"/>
                                    <StackPanel Grid.Column="2" Grid.Row="1" HorizontalAlignment="Center">
                                        <telerik:RadButton telerik:LocalizationManager.ResourceKey="Spreadsheet_CustomSortListsDialog_Add" Margin="10 0 0 10" Command="{Binding Path=AddCommand}" MinWidth="75"/>
                                        <telerik:RadButton telerik:LocalizationManager.ResourceKey="Spreadsheet_CustomSortListsDialog_Delete" Margin="10 0 0 0" Command="{Binding Path=DeleteCommand}" MinWidth="75"/>
                                    </StackPanel>
                                </Grid>
                            </telerik:RadTabItem>
                        </telerik:RadTabControl>
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Grid.Row="1" Margin="5">
                            <telerik:RadButton telerik:LocalizationManager.ResourceKey="Ok" Command="{Binding Path=OKCommand}" Margin="10 5 0 0" MinWidth="75"/>
                            <telerik:RadButton telerik:LocalizationManager.ResourceKey="Cancel" Command="{Binding Path=CancelCommand}" Margin="10 5 0 0" MinWidth="75"/>
                        </StackPanel>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
