    <Style x:Key="CustomFilterDialogContentStyle" TargetType="dialogs:CustomFilterDialogContent">
        <Setter Property="Header" Value="{Binding DialogHeader}"/>
        <Setter Property="Background" Value="{telerik:Windows8Resource ResourceKey=MainBrush}"/>
        <Setter Property="Padding" Value="5"/>
        <Setter Property="ResizeMode" Value="NoResize"/>
