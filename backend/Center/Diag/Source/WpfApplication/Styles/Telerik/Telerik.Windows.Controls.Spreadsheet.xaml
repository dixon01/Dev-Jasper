    <Style x:Key="AdornerControlStyle" TargetType="spreadsheetControls:AdornerControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <Grid x:Name="AdornerRoot">
                        <Grid x:Name="gridControl" RenderTransformOrigin="0.5,0.5" ShowGridLines="False" Background="Transparent">
                            <Grid.RenderTransform>
                                <TransformGroup>
                                    <ScaleTransform ScaleX="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsHorizontallyFlipped, Converter={StaticResource BoolToIntegerConverter}}" ScaleY="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsVerticallyFlipped, Converter={StaticResource BoolToIntegerConverter}}"/>
                                    <RotateTransform Angle="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=RotationAngle}"/>
                                </TransformGroup>
                            </Grid.RenderTransform>
                            <Grid.Resources>
                                <Style TargetType="Ellipse" x:Key="ResizeStyle">
                                    <Setter Property="Margin" Value="-5"/>
                                    <Setter Property="Width" Value="10"/>
                                    <Setter Property="Height" Value="10"/>
                                    <Setter Property="StrokeThickness" Value="1"/>
                                    <Setter Property="Stroke" Value="#4D7399"/>
                                    <Setter Property="Fill">
                                        <Setter.Value>
                                            <LinearGradientBrush StartPoint="0.5 0.2" EndPoint="0.5 0.8">
                                                <GradientStop Offset="0" Color="#FFFFFF"/>
                                                <GradientStop Offset="0.5" Color="#CAE9EC"/>
                                                <GradientStop Offset="1" Color="#FFFFFF"/>
                                            </LinearGradientBrush>
                                        </Setter.Value>
                                    </Setter>
                                </Style>
                                <Style TargetType="Rectangle" x:Key="ResizeNoEdgeStyle">
                                    <Setter Property="Margin" Value="-4"/>
                                    <Setter Property="Width" Value="8"/>
                                    <Setter Property="Height" Value="8"/>
                                    <Setter Property="StrokeThickness" Value="1"/>
                                    <Setter Property="Stroke" Value="#4D7399"/>
                                    <Setter Property="Fill">
                                        <Setter.Value>
                                            <LinearGradientBrush StartPoint="0.5 0.2" EndPoint="0.5 0.8">
                                                <GradientStop Offset="0" Color="#FFFFFF"/>
                                                <GradientStop Offset="0.5" Color="#CAE9EC"/>
                                                <GradientStop Offset="1" Color="#FFFFFF"/>
                                            </LinearGradientBrush>
                                        </Setter.Value>
                                    </Setter>
                                </Style>
                                <Style TargetType="Ellipse" x:Key="RotateStyle">
                                    <Setter Property="Margin" Value="-24"/>
                                    <Setter Property="Width" Value="10"/>
                                    <Setter Property="Height" Value="10"/>
                                    <Setter Property="StrokeThickness" Value="1"/>
                                    <Setter Property="Stroke" Value="#4D7399"/>
                                    <Setter Property="Fill">
                                        <Setter.Value>
                                            <LinearGradientBrush StartPoint="0.5 0.2" EndPoint="0.5 0.8">
                                                <GradientStop Offset="0" Color="#C4FF92"/>
                                                <GradientStop Offset="0.5" Color="#85E137"/>
                                                <GradientStop Offset="1" Color="#C4FF92"/>
                                            </LinearGradientBrush>
                                        </Setter.Value>
                                    </Setter>
                                </Style>
                            </Grid.Resources>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="0"/>
                                <RowDefinition/>
                                <RowDefinition Height="0"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="0"/>
                                <ColumnDefinition/>
                                <ColumnDefinition Width="0"/>
                            </Grid.ColumnDefinitions>
                            <Image x:Name="ImagePreview" Stretch="Fill" Grid.Row="1" Grid.Column="1" Visibility="Collapsed" Opacity="1"/>
                            <Rectangle StrokeThickness="0" Width="1" Fill="#4D7399" Grid.RowSpan="3" HorizontalAlignment="Left" Grid.Column="1" VerticalAlignment="Stretch"/>
                            <Rectangle StrokeThickness="0" Width="1" Fill="#4D7399" Grid.RowSpan="3" HorizontalAlignment="Right" Grid.Column="1" VerticalAlignment="Stretch"/>
                            <Rectangle StrokeThickness="0" Height="1" Fill="#4D7399" Grid.ColumnSpan="3" HorizontalAlignment="Stretch" Grid.Row="1" VerticalAlignment="Top"/>
                            <Rectangle StrokeThickness="0" Height="1" Fill="#4D7399" Stroke="Black" Grid.ColumnSpan="3" HorizontalAlignment="Stretch" Grid.Row="1" VerticalAlignment="Bottom"/>
                            <Rectangle x:Name="rotateLine"
                                    StrokeThickness="0"
                                    Height="24"
                                    Width="1"
                                    Fill="#4D7399"
                                    Grid.ColumnSpan="3"
                                    HorizontalAlignment="Center"
                                    Grid.Row="1"
                                    VerticalAlignment="Top"
                                    Margin="0 -20 0 0"/>
                            <Ellipse x:Name="Rotate" Style="{StaticResource RotateStyle}" Stroke="#4D7399" HorizontalAlignment="Center" VerticalAlignment="Top" Grid.ColumnSpan="3" Grid.RowSpan="3"/>
                            <Ellipse x:Name="Resize00" Style="{StaticResource ResizeStyle}" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                            <Ellipse x:Name="Resize10" Style="{StaticResource ResizeStyle}" Grid.Column="2" HorizontalAlignment="Right" VerticalAlignment="Top"/>
                            <Ellipse x:Name="Resize01" Style="{StaticResource ResizeStyle}" Grid.Row="2" HorizontalAlignment="Left" VerticalAlignment="Bottom"/>
                            <Ellipse x:Name="Resize11" Style="{StaticResource ResizeStyle}" Grid.Column="2" Grid.Row="2" HorizontalAlignment="Right" VerticalAlignment="Bottom"/>
                            <Rectangle x:Name="ResizeTop" Style="{StaticResource ResizeNoEdgeStyle}" VerticalAlignment="Top" Grid.ColumnSpan="3" Grid.RowSpan="3"/>
                            <Rectangle x:Name="ResizeBottom" Style="{StaticResource ResizeNoEdgeStyle}" VerticalAlignment="Bottom" Grid.ColumnSpan="3" Grid.RowSpan="3"/>
                            <Rectangle x:Name="ResizeLeft" Style="{StaticResource ResizeNoEdgeStyle}" HorizontalAlignment="Left" Grid.ColumnSpan="3" Grid.RowSpan="3"/>
                            <Rectangle x:Name="ResizeRight" Style="{StaticResource ResizeNoEdgeStyle}" HorizontalAlignment="Right" Grid.ColumnSpan="3" Grid.RowSpan="3"/>
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="FunctionsMenuStyle" TargetType="spreadsheetControls:FunctionsMenu">
        <Setter Property="FontFamily" Value="{telerik:Windows8Resource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik:Windows8Resource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik:Windows8Resource ResourceKey=MarkerBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:Windows8Resource ResourceKey=BasicBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="spreadsheetControls:FunctionsMenu">
                    <telerik:RadContextMenu BorderThickness="0" Padding="0" Margin="0">
                        <telerik:RadMenuGroupItem
                                Background="Transparent"
                                ItemsSource="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Items}"
                                DataContext="{Binding}"
                                IsEnabled="{Binding Path=ApplyFunction.IsEnabled}"
                                ScrollViewer.VerticalScrollBarVisibility="Auto"
                                MaxHeight="500"
                                StaysOpenOnClick="False"
                                MinWidth="120">
                            <telerik:RadMenuGroupItem.ItemContainerTemplateSelector>
                                <spreadsheetUtilities:SingleTemplateSelector>
                                    <spreadsheetUtilities:SingleTemplateSelector.MenuItemTemplate>
                                        <DataTemplate>
                                            <telerik:RadMenuItem
                                                    Header="{Binding Name}"
                                                    CommandParameter="{Binding Name}"
                                                    Command="{Binding Path=DataContext.ApplyFunction.Command, RelativeSource={RelativeSource AncestorType=spreadsheetControls:FunctionsMenu}}"
                                                    IsEnabled="{Binding Path=DataContext.ApplyFunction.IsEnabled, RelativeSource={RelativeSource AncestorType=spreadsheetControls:FunctionsMenu}}">
                                                <ToolTipService.ToolTip>
                                                    <ToolTip
                                                            Background="{telerik:Windows8Resource ResourceKey=MainBrush}"
                                                            BorderBrush="{telerik:Windows8Resource ResourceKey=BasicBrush}"
                                                            Foreground="{telerik:Windows8Resource ResourceKey=StrongBrush}"
                                                            BorderThickness="1">
                                                        <StackPanel>
                                                            <TextBlock Text="{Binding Name}" FontWeight="Bold" Margin="0 4 0 0" FontSize="11"/>
                                                            <TextBlock Text="{Binding Path=FunctionInfo.Description}" TextWrapping="Wrap" MaxWidth="200" Margin="4" FontSize="11"/>
                                                        </StackPanel>
                                                    </ToolTip>
                                                </ToolTipService.ToolTip>
                                            </telerik:RadMenuItem>
                                        </DataTemplate>
                                    </spreadsheetUtilities:SingleTemplateSelector.MenuItemTemplate>
                                </spreadsheetUtilities:SingleTemplateSelector>
                            </telerik:RadMenuGroupItem.ItemContainerTemplateSelector>
                        </telerik:RadMenuGroupItem>
                        <telerik:RadMenuItem
                                telerik:LocalizationManager.ResourceKey="Spreadsheet_FunctionsMenu_InsertFunction"
                                Command="{Binding Path=ShowInsertFunctionDialog.Command}"
                                IsEnabled="{Binding Path=ShowInsertFunctionDialog.IsEnabled}"
                                CommandParameter="{Binding Path=Category, RelativeSource={RelativeSource TemplatedParent}}">
                            <telerik:RadMenuItem.Icon>
                                <Image Source="{telerik:IconResource IconRelativePath=16/insertFunction.png, IconSources={StaticResource IconPaths}}" UseLayoutRounding="True" Stretch="None"/>
                            </telerik:RadMenuItem.Icon>
                        </telerik:RadMenuItem>
                    </telerik:RadContextMenu>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="FilteringPopupControlStyle" TargetType="spreadsheetControls:FilteringPopupControl">
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="Focusable" Value="False"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <telerik:RadDropDownButton x:Name="PART_DropDownButton"
                            Command="{Binding InitializePopupCommand}"
                            IsOpen="{Binding Path=IsPopupOpened, Mode=TwoWay}"
                            IsEnabled="{Binding Path=CanFilter}"
                            IsTabStop="False"
                            DropDownIndicatorVisibility="Collapsed"
                            Focusable="False">
                        <telerik:RadDropDownButton.Content>
                            <StackPanel>
                                <Image
