                        <telerik:RadComboBox
                                Grid.Row="1"
                                HorizontalAlignment="Stretch"
                                Margin="0 5"
                                ItemsSource="{Binding Path=TopBottomItemsSource}"
                                SelectedValue="{Binding Path=SelectedTopBottom, Mode=TwoWay}">
                            <telerik:RadComboBox.ItemTemplate>
                                <DataTemplate>
                                    <TextBlock Text="{Binding Converter={StaticResource TopBottomFilterTypeToStringConverter}}"/>
                                </DataTemplate>
                            </telerik:RadComboBox.ItemTemplate>
                        </telerik:RadComboBox>
                        <telerik:RadNumericUpDown Grid.Row="1" Grid.Column="1" Value="{Binding Path=SelectedValue, Mode=TwoWay}" IsInteger="True" Margin="5"/>
                        <telerik:RadComboBox
                                Grid.Row="1"
                                Grid.Column="2"
                                Margin="0 5"
                                HorizontalAlignment="Stretch"
                                ItemsSource="{Binding Path=ItemsPercentItemsSource}"
                                SelectedValue="{Binding Path=SelectedItemsPercent, Mode=TwoWay}">
                            <telerik:RadComboBox.ItemTemplate>
                                <DataTemplate>
                                    <TextBlock Text="{Binding Converter={StaticResource ItemsPercentFilterTypeToStringConverter}}"/>
                                </DataTemplate>
                            </telerik:RadComboBox.ItemTemplate>
                        </telerik:RadComboBox>
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Grid.Row="2" Grid.ColumnSpan="3" VerticalAlignment="Bottom">
                            <telerik:RadButton telerik:LocalizationManager.ResourceKey="Ok" Margin="10 5 0 0" x:Name="PART_ButtonOK" Command="{Binding Path=OKCommand}" MinWidth="75"/>
                            <telerik:RadButton telerik:LocalizationManager.ResourceKey="Cancel" Margin="10 5 0 0" x:Name="PART_ButtonCancel" Command="{Binding Path=CancelCommand}" MinWidth="75"/>
                        </StackPanel>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
