                                        <TextBlock Text="Column" Margin="5 2"/>
                                    </Border>
                                    <Border BorderBrush="{telerik:Windows8Resource ResourceKey=BasicBrush}" BorderThickness="0 0 1 0" Grid.Column="1">
                                        <TextBlock Text="Sort On" Margin="5 2"/>
                                    </Border>
                                    <Border Grid.Column="2">
                                        <TextBlock Text="Order" Margin="5 2"/>
                                    </Border>
                                </Grid>
