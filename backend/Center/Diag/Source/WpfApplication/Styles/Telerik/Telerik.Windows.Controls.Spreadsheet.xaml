        <Setter Property="FontSize" Value="{telerik:Windows8Resource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik:Windows8Resource ResourceKey=MarkerBrush}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="dialogs:SortingDialogContent">
                    <Grid
                            Margin="{TemplateBinding Padding}"
                            Background="{TemplateBinding Background}"
                            HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                            VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <StackPanel Orientation="Horizontal" Grid.Row="0">
                            <telerik:RadButton Command="{Binding Path=AddLevelCommand}" MinWidth="83" Margin="1 5 2 5">
                                <telerik:RadButton.Content>
                                    <StackPanel Orientation="Horizontal">
                                        <Image Stretch="None" Source="{telerik:IconResource IconRelativePath=16/addLevel.png, IconSources={StaticResource IconPaths}}"/>
                                        <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_SortingDialog_AddLevel" Margin="10 0 0 0"/>
                                    </StackPanel>
                                </telerik:RadButton.Content>
                            </telerik:RadButton>
                            <telerik:RadButton Command="{Binding Path=DeleteLevelCommand}" MinWidth="83" Margin="2 5">
                                <telerik:RadButton.Content>
                                    <StackPanel Orientation="Horizontal">
                                        <Image Source="/Telerik.Windows.Controls.Spreadsheet;component/Images/Light/16/cancelFormula.png" Width="16" Stretch="None"/>
                                        <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_SortingDialog_DeleteLevel" Margin="10 0 0 0"/>
                                    </StackPanel>
                                </telerik:RadButton.Content>
                            </telerik:RadButton>
                            <telerik:RadButton Command="{Binding Path=CopyLevelCommand}" MinWidth="83" Margin="2 5">
                                <telerik:RadButton.Content>
                                    <StackPanel Orientation="Horizontal">
                                        <Image Stretch="None" Source="{telerik:IconResource IconRelativePath=16/copy.png, IconSources={StaticResource IconPaths}}"/>
                                        <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_SortingDialog_CopyLevel" Margin="10 0 0 0"/>
                                    </StackPanel>
                                </telerik:RadButton.Content>
                            </telerik:RadButton>
                            <Border BorderBrush="Black" Width="1" Height="24" BorderThickness="1" HorizontalAlignment="Left" VerticalAlignment="Stretch"/>
                            <telerik:RadButton Command="{Binding Path=MoveUpCommand}" MinWidth="24" Margin="2 5" Width="24">
                                <telerik:RadButton.Content>
                                    <Path RenderTransformOrigin="0.5 0.5" Data="M0,0 L6,0 L3,4 z" Fill="{telerik:Windows8Resource ResourceKey=StrongBrush}">
                                        <Path.RenderTransform>
                                            <TransformGroup>
                                                <ScaleTransform/>
                                                <SkewTransform/>
                                                <RotateTransform Angle="180"/>
                                                <TranslateTransform/>
                                            </TransformGroup>
                                        </Path.RenderTransform>
                                    </Path>
                                </telerik:RadButton.Content>
                            </telerik:RadButton>
                            <telerik:RadButton Command="{Binding Path=MoveDownCommand}" MinWidth="24" Margin="2 5" Width="24">
                                <telerik:RadButton.Content>
                                    <Path Data="M0,0 L6,0 L3,4 z" Fill="{telerik:Windows8Resource ResourceKey=StrongBrush}"/>
                                </telerik:RadButton.Content>
                            </telerik:RadButton>
                        </StackPanel>
