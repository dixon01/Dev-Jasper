    <SolidColorBrush x:Key="ThemeControlLightBrush" Color="#FFDADADA"/>
    <SolidColorBrush x:Key="ThemeControlLowBrush" Color="#FF9FADBF"/>
    <SolidColorBrush x:Key="ThemeControlMediumBrush" Color="#FF616C81"/>
    <SolidColorBrush x:Key="ThemeControlHighBrush" Color="#FF344C64"/>
    <SolidColorBrush x:Key="ThemeControlMainBackgroundBrush" Color="#FFFFFFFF"/>
    <SolidColorBrush x:Key="ThemeControlPathFillBrush" Color="#FFE4EBF5"/>
    <SolidColorBrush x:Key="ThemeControlRectangleStrokeBrush" Color="#FF868686"/>
    <SolidColorBrush x:Key="ThemeControlFontPreviewLightBrush" Color="#FFF5F5F5"/>
    <SolidColorBrush x:Key="ThemeControlFontPreviewDarkBrush" Color="#FFC3C8D5"/>
    <LinearGradientBrush x:Key="ThemeControlFontSmallPreviewRectangleGradientBrush" StartPoint="0 0">
        <GradientStop Color="#FFFCFDFE" Offset="0"/>
        <GradientStop Color="#FFD4DEEE" Offset="1"/>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="SelectedFillBrush" Color="#FFAAAAAA"/>
    <SolidColorBrush x:Key="HoverFillBrush" Color="#FF919191"/>
    <SolidColorBrush x:Key="FullySelectedFillBrush" Color="#FFAAAAAA"/>
    <SolidColorBrush x:Key="RectangleStrokeBrush" Color="#FFB1B5BA"/>
    <LinearGradientBrush x:Key="SaturationValueBrush1" EndPoint="1,0.5" StartPoint="0,0.5">
        <GradientStop Color="#FFFFFFFF"/>
        <GradientStop Color="#00FFFFFF" Offset="1"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="SaturationValueBrush2" EndPoint="0.5,0" StartPoint="0.5,1">
        <GradientStop Color="#FF000000" Offset="0"/>
        <GradientStop Color="#00000000" Offset="1"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="HueBrushHorizontal" EndPoint="1,0.5" StartPoint="0, 0.5">
        <GradientStop Color="#FFFF0000"/>
        <GradientStop Color="#FEFFFF00" Offset="0.167"/>
        <GradientStop Color="#FE00FF00" Offset="0.333"/>
        <GradientStop Color="#FE00FFFF" Offset="0.5"/>
        <GradientStop Color="#FE0000FF" Offset="0.667"/>
        <GradientStop Color="#FEFF00FF" Offset="0.833"/>
        <GradientStop Color="#FFFF0000" Offset="1.0"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="ZeroGrayBrush" EndPoint="0.5, 1" StartPoint="0.5, 0">
        <GradientStop Color="Transparent" Offset="0"/>
        <GradientStop Color="Gray" Offset="1"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="HueBrush" EndPoint="0.5,1" StartPoint="0.5,0.025">
        <GradientStop Color="#FFFF0000"/>
        <GradientStop Color="#FEFFFF00" Offset="0.167"/>
        <GradientStop Color="#FE00FF00" Offset="0.333"/>
        <GradientStop Color="#FE00FFFF" Offset="0.5"/>
        <GradientStop Color="#FE0000FF" Offset="0.667"/>
        <GradientStop Color="#FEFF00FF" Offset="0.833"/>
        <GradientStop Color="#FFFF0000" Offset="1.0"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="ZeroToWhiteBrush" EndPoint="0.5,0" StartPoint="0.5, 1">
        <GradientStop Color="Transparent" Offset="0.1"/>
        <GradientStop Color="White" Offset="0.9"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="ZeroToBlackBrush" EndPoint="0.5,1" StartPoint="0.5, 0">
        <GradientStop Color="Transparent" Offset="0.1"/>
        <GradientStop Color="Black" Offset="0.9"/>
    </LinearGradientBrush>
