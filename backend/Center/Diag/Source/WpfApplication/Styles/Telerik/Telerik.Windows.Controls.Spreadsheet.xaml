    <Style x:Key="SelectAllControlStyle" TargetType="worksheetMargins:SelectAllControl">
        <Setter Property="SelectedFillBrush" Value="{StaticResource SelectedFillBrush}"/>
        <Setter Property="NormalFillBrush" Value="{telerik:Windows8Resource ResourceKey=MainBrush}"/>
        <Setter Property="HoverFillBrush" Value="{StaticResource HoverFillBrush}"/>
        <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
        <Setter Property="VerticalContentAlignment" Value="Stretch"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="worksheetMargins:SelectAllControl">
                    <Grid HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                        <Rectangle Fill="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=NormalFillBrush}" Stroke="{StaticResource RectangleStrokeBrush}" StrokeThickness="0.3"/>
                        <Rectangle Fill="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Fill}" Margin="1.5"/>
                        <Rectangle x:Name="PART_TopRectangle" Margin="4" Fill="{StaticResource RectangleStrokeBrush}"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadWorksheetEditorStyle" TargetType="worksheet:RadWorksheetEditor">
        <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
        <Setter Property="VerticalContentAlignment" Value="Stretch"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="worksheet:RadWorksheetEditor">
                    <Grid x:Name="LayoutRoot" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <worksheetMargins:SelectAllControl x:Name="SelectAllControl" AssociatedWorksheetEditor="{Binding RelativeSource={RelativeSource TemplatedParent}}"/>
                        <StackPanel x:Name="TopMarginsPanel" Grid.Column="1" HorizontalAlignment="Stretch" VerticalAlignment="Top"/>
                        <StackPanel x:Name="LeftMarginsPanel" Grid.Row="1" Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Stretch"/>
                        <ContentControl x:Name="ContentPresenter"
                                Grid.Row="1"
                                Grid.Column="1"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Stretch"
                                HorizontalContentAlignment="Stretch"
                                VerticalContentAlignment="Stretch"
                                IsTabStop="False"
                                Focusable="False"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="ResizeThumbStyle" TargetType="worksheetMargins:ResizeThumb">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="worksheetMargins:ResizeThumb">
                    <Grid>
                        <Thumb x:Name="PART_Thumb">
                            <Thumb.Template>
                                <ControlTemplate>
                                    <Rectangle Fill="Transparent"/>
                                </ControlTemplate>
                            </Thumb.Template>
                        </Thumb>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="rowColumnheadingBaseStyle" TargetType="worksheetMargins:RowColumnHeadingBase">
        <Setter Property="NormalFillBrush" Value="{telerik:Windows8Resource ResourceKey=MainBrush}"/>
        <Setter Property="FullySelectedFillBrush" Value="{StaticResource FullySelectedFillBrush}"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
        <Setter Property="VerticalContentAlignment" Value="Stretch"/>
        <Setter Property="RectangleStrokeBrush" Value="{StaticResource RectangleStrokeBrush}"/>
        <Setter Property="RectangleStrokeThickness" Value="0.3"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="UseLayoutRounding" Value="True"/>
        <Setter Property="FontSize" Value="{telerik:Windows8Resource ResourceKey=FontSize}"/>
        <Setter Property="FontFamily" Value="{telerik:Windows8Resource ResourceKey=FontFamily}"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="Foreground" Value="{telerik:Windows8Resource ResourceKey=MarkerBrush}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="worksheetMargins:RowColumnHeadingBase">
                    <Grid x:Name="Content">
                        <Rectangle
                                Stroke="{TemplateBinding RectangleStrokeBrush}"
                                StrokeThickness="{TemplateBinding RectangleStrokeThickness}"
                                MinWidth="25"
                                Fill="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Fill}"
                                SnapsToDevicePixels="True"/>
                        <TextBlock Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Text}" HorizontalAlignment="Center" VerticalAlignment="Center" Padding="3 2"/>
                        <worksheetMargins:ResizeThumb x:Name="PART_FirstResizeThumb"/>
                        <worksheetMargins:ResizeThumb x:Name="PART_SecondResizeThumb"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RowHeadingStyle" TargetType="worksheetMargins:RowHeading" BasedOn="{StaticResource rowColumnheadingBaseStyle}">
        <Setter Property="SelectedFillBrush" Value="{telerik:Windows8Resource ResourceKey=AccentBrush}"/>
        <Setter Property="HoverFillBrush" Value="{telerik:Windows8Resource ResourceKey=StrongBrush}"/>
    </Style>
    <Style x:Key="ColumnHeadingStyle" TargetType="worksheetMargins:ColumnHeading" BasedOn="{StaticResource rowColumnheadingBaseStyle}">
        <Setter Property="SelectedFillBrush" Value="{telerik:Windows8Resource ResourceKey=AccentBrush}"/>
        <Setter Property="HoverFillBrush" Value="{telerik:Windows8Resource ResourceKey=StrongBrush}"/>
    </Style>
    <Style x:Key="printRowColumnHeadingBaseStyle" TargetType="worksheetMargins:RowColumnHeadingBase" BasedOn="{StaticResource rowColumnheadingBaseStyle}">
        <Setter Property="NormalFillBrush" Value="White"/>
        <Setter Property="Fill" Value="White"/>
        <Setter Property="RectangleStrokeBrush" Value="Black"/>
        <Setter Property="Foreground" Value="Black"/>
        <Setter Property="RectangleStrokeThickness" Value="0.5"/>
    </Style>
    <Style x:Key="PrintRowHeadingStyle" TargetType="worksheetMargins:PrintRowHeading" BasedOn="{StaticResource printRowColumnHeadingBaseStyle}"/>
    <Style x:Key="PrintColumnHeadingStyle" TargetType="worksheetMargins:PrintColumnHeading" BasedOn="{StaticResource printRowColumnHeadingBaseStyle}"/>
    <Style x:Key="CellEditorStyle" TargetType="worksheet:CellEditor">
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="worksheet:CellEditor">
                    <Grid x:Name="RootElement">
                        <Border x:Name="Border" BorderThickness="{TemplateBinding BorderThickness}" Opacity="1" Background="{TemplateBinding Background}">
                            <ScrollViewer
                                    VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
                                    Padding="{TemplateBinding Padding}"
                                    BorderThickness="0"
                                    IsTabStop="False"
                                    Name="PART_ContentHost"/>
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="SheetSelectorTabItemStyle" TargetType="spreadsheetControls:SheetSelectorTabItem">
        <Setter Property="DefaultBackground" Value="{telerik:Windows8Resource ResourceKey=MainBrush}"/>
        <Setter Property="UseLayoutRounding" Value="True"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="spreadsheetControls:SheetSelectorTabItem">
                    <Grid x:Name="root">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Active">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_TabNameTextBlock" Storyboard.TargetProperty="(TextElement.FontWeight)">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <FontWeight>Bold</FontWeight>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="SelectedCellIndicator" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="SelectedCellUpperIndicator" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="SelectedBorder" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="SelectedBorder" Storyboard.TargetProperty="Margin">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Thickness>0 1 -1 0</Thickness>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="NormalBorder" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Collapsed</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Separator" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Collapsed</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_TabNameTextBlock" Storyboard.TargetProperty="Foreground">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{telerik:Windows8Resource ResourceKey=AccentBrush}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Normal">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_TabNameTextBlock" Storyboard.TargetProperty="(TextElement.FontWeight)">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <FontWeight>Normal</FontWeight>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_TabNameTextBlock" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_TabNameTextBox" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Collapsed</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Edit">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_TabNameTextBlock" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Collapsed</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_TabNameTextBox" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_TabNameTextBox" Storyboard.TargetProperty="(TextElement.FontWeight)">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <FontWeight>Bold</FontWeight>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Disabled">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0" To="0.5" Storyboard.TargetName="root" Storyboard.TargetProperty="Opacity"/>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="10"/>
                        </Grid.ColumnDefinitions>
                        <Border
                                Grid.ColumnSpan="3"
                                x:Name="NormalBorder"
                                Margin="1"
                                BorderThickness="0 1 0 0"
                                Background="{telerik:Windows8Resource ResourceKey=MainBrush}"
                                BorderBrush="{telerik:Windows8Resource ResourceKey=BasicBrush}"/>
                        <Rectangle x:Name="Separator" Width="1" Margin="0 3" VerticalAlignment="Stretch" HorizontalAlignment="Left" Fill="{telerik:Windows8Resource ResourceKey=BasicBrush}"/>
                        <Border Grid.ColumnSpan="3" x:Name="CurrentBackgroundBorder" Background="{Binding CurrentBackground}" Margin="2 2 1 1"/>
                        <Border Background="Transparent" BorderThickness="0 0 0 1" Margin="0 2 0 0" x:Name="PART_BorderCenterOuter" Grid.Column="1">
                            <Grid>
                                <TextBlock x:Name="PART_TabNameTextBlock"
                                        Text="{Binding Path=SheetName, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                        TextAlignment="Center"
                                        VerticalAlignment="Center"
                                        HorizontalAlignment="Center"
                                        FontSize="12"/>
                                <TextBox x:Name="PART_TabNameTextBox"
                                        Background="{Binding CurrentBackground}"
                                        Text="{Binding Path=SheetName, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                        BorderThickness="0"
                                        Visibility="Collapsed"
                                        Height="23"
                                        VerticalAlignment="Center"
                                        FontSize="12"
                                        MaxLength="31"/>
                                <Image x:Name="PART_Image"
                                        VerticalAlignment="Center"
                                        HorizontalAlignment="Right"
                                        Width="16"
                                        Height="16"
                                        UseLayoutRounding="True"
                                        Source="/Telerik.Windows.Controls.Spreadsheet;component/Images/Light/16/newSheet.png"/>
                            </Grid>
                        </Border>
                        <Border x:Name="SelectedBorder"
                                BorderThickness="1 0 1 1"
                                Background="Transparent"
                                Grid.ColumnSpan="3"
                                Visibility="Collapsed"
                                IsHitTestVisible="False"
                                BorderBrush="{telerik:Windows8Resource ResourceKey=BasicBrush}"/>
                        <Rectangle x:Name="SelectedCellIndicator"
                                Grid.ColumnSpan="3"
                                Visibility="Collapsed"
                                Height="2"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Bottom"
                                Fill="{telerik:Windows8Resource ResourceKey=AccentBrush}"/>
                        <Rectangle x:Name="SelectedCellUpperIndicator"
                                Grid.ColumnSpan="3"
                                Margin="0 1 0 0"
                                Visibility="Collapsed"
                                Height="1"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Top"
                                Fill="{telerik:Windows8Resource ResourceKey=MainBrush}"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadSpreadsheetSheetSelectorStyle" TargetType="spreadsheetControls:RadSpreadsheetSheetSelector">
        <Setter Property="UseLayoutRounding" Value="True"/>
        <Setter Property="Background" Value="{telerik:Windows8Resource ResourceKey=MainBrush}"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="spreadsheetControls:RadSpreadsheetSheetSelector">
                    <Grid Background="{TemplateBinding Background}" DataContext="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=RadSpreadsheet.CommandDescriptors}">
                        <Rectangle Height="1" VerticalAlignment="Top" Fill="{telerik:Windows8Resource ResourceKey=BasicBrush}"/>
                        <StackPanel Orientation="Horizontal" ScrollViewer.HorizontalScrollBarVisibility="Hidden">
                            <telerik:RadButton x:Name="PART_GoToFirstButton"
                                    Width="16"
                                    Margin="0"
                                    CornerRadius="0"
                                    BorderThickness="0 1 0 0"
                                    BorderBrush="{telerik:Windows8Resource ResourceKey=BasicBrush}"
                                    IsTabStop="False">
                                <telerik:RadButton.Content>
                                    <Image
                                            Source="/Telerik.Windows.Controls.Spreadsheet;component/Images/Light/SheetSelector/firstSheet.png"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            Stretch="None"/>
                                </telerik:RadButton.Content>
                            </telerik:RadButton>
                            <telerik:RadButton x:Name="PART_GoToPreviousButton"
                                    Width="16"
                                    Margin="0"
                                    CornerRadius="0"
                                    BorderThickness="0 1 0 0"
                                    BorderBrush="{telerik:Windows8Resource ResourceKey=BasicBrush}"
                                    IsTabStop="False">
                                <telerik:RadButton.Content>
                                    <Image
                                            Source="/Telerik.Windows.Controls.Spreadsheet;component/Images/Light/SheetSelector/previousSheet.png"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            Stretch="None"/>
                                </telerik:RadButton.Content>
                            </telerik:RadButton>
                            <telerik:RadButton x:Name="PART_GoToNextButton"
                                    Width="16"
                                    Margin="0"
