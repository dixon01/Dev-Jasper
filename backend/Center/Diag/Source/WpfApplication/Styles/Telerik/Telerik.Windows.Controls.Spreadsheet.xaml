    <spreadsheetControls:ScalingMethodToBooleanConverter x:Key="ScalingMethodToBooleanConverter"/>
    <spreadsheetControls:PageOrderToBooleanConverter x:Key="PageOrderToBooleanConverter"/>
    <spreadsheetControls:PatternTypeToIntConverter x:Key="PatternTypeToIntConverter"/>
    <spreadsheetControls:ColorToThemableColorConverter x:Key="ColorToThemableColorConverter"/>
    <spreadsheetControls:ShadingStyleToBooleanConverter x:Key="ShadingStyleToBooleanConverter"/>
    <spreadsheetControls:FillTabStateToVisibilityConverter x:Key="FillTabStateToVisibilityConverter"/>
    <spreadsheetControls:RadHorizontalAlignmentToStringConverter x:Key="RadHorizontalAlignmentToStringConverter"/>
    <spreadsheetControls:RadVerticalAlignmentToStringConverter x:Key="RadVerticalAlignmentToStringConverter"/>
    <spreadsheetControls:FormatStringCategoryToStringConverter x:Key="FormatStringCategoryToStringConverter"/>
    <spreadsheetControls:ThemeFontTypeToTextConverter x:Key="themeFontTypeToTextConverter"/>
    <spreadsheetControls:RemoveAlphaFromColorStringConverter x:Key="RemoveAlphaFromColorStringConverter"/>
    <spreadsheetControls:UnderlineTypesToStringConverter x:Key="UnderlineTypesToStringConverter"/>
    <spreadsheetControls:SeriesTypeToBooleanConverter x:Key="SeriesTypeToBooleanConverter"/>
    <spreadsheetControls:SeriesOrientationToBooleanConverter x:Key="SeriesOrientationToBooleanConverter"/>
    <spreadsheetControls:SeriesDateUnitToBooleanConverter x:Key="SeriesDateUnitToBooleanConverter"/>
    <spreadsheetControls:BooleanToInversedBooleanConverter x:Key="BooleanToInversedBooleanConverter"/>
    <spreadsheetControls:CellIndexToNameConverter x:Key="CellIndexToNameConverter"/>
    <spreadsheetControls:IntToColumnNameConverter x:Key="IntToColumnNameConverter"/>
    <spreadsheetControls:SortTypeToStringConverter x:Key="SortTypeToStringConverter"/>
    <spreadsheetControls:DoubleToPercentConverter x:Key="doubleToPercentConverter"/>
    <spreadsheetControls:CellValueFormatToNumberFormatInfoConverter x:Key="cellValueFormatToNumberFormatInfoConverter"/>
    <spreadsheetControls:BoolToVisibilityValueConverter x:Key="BoolToVisibilityValueConverter"/>
    <spreadsheetControls:InversedBoolToVisibilityValueConverter x:Key="InversedBoolToVisibilityValueConverter"/>
    <spreadsheetControls:BoolToIntegerConverter x:Key="BoolToIntegerConverter"/>
    <telerik:BooleanToOpacityConverter x:Key="BooleanToOpacityConverter"/>
    <telerik:InvertedBooleanToVisibilityConverter x:Key="InvertedBooleanToVisibilityConverter"/>
    <spreadsheetControls:PatternFillToBackgroundColorConverter x:Key="PatternFillToBackgroundColorConverter"/>
    <spreadsheetControls:PatternFillToPatternColorConverter x:Key="PatternFillToPatternColorConverter"/>
    <spreadsheetControls:LogicalOperatorToBooleanConverter x:Key="LogicalOperatorToBooleanConverter"/>
    <spreadsheetControls:TopBottomFilterTypeToStringConverter x:Key="TopBottomFilterTypeToStringConverter"/>
    <spreadsheetControls:ItemsPercentFilterTypeToStringConverter x:Key="ItemsPercentFilterTypeToStringConverter"/>
    <spreadsheetControls:ComparisonOperatorItemToStringConverter x:Key="ComparisonOperatorItemToStringConverter"/>
    <spreadsheetControls:FilterToVisibilityConverter x:Key="FilterToVisibilityConverter"/>
    <Style x:Key="LinkButtonStyle" TargetType="Button">
        <Setter Property="Foreground" Value="{telerik:Windows8Resource ResourceKey=MarkerBrush}"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="FontFamily" Value="{telerik:Windows8Resource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="12"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Button">
                    <Grid>
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal"/>
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="Visibility" Storyboard.TargetName="decorator">
                                            <DiscreteObjectKeyFrame KeyTime="00:00:00">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="Visibility" Storyboard.TargetName="presenter">
                                            <DiscreteObjectKeyFrame KeyTime="00:00:00">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Collapsed</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(FrameworkElement.Cursor)" Storyboard.TargetName="decorator">
                                            <DiscreteObjectKeyFrame KeyTime="00:00:00">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Cursor>Hand</Cursor>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <TextBlock x:Name="presenter" Foreground="{TemplateBinding Foreground}" Text="{TemplateBinding Content}"/>
                        <TextBlock x:Name="decorator"
                                Foreground="{telerik:Windows8Resource ResourceKey=AccentBrush}"
                                TextDecorations="Underline"
                                Visibility="Collapsed"
                                Text="{TemplateBinding Content}"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadColorPaletteViewItemStylePredefined" TargetType="telerik:RadColorPaletteViewItem">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadColorPaletteViewItem">
                    <Grid x:Name="RootElement" Margin="0">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="FocusStates">
                                <VisualState x:Name="Unfocused"/>
                                <VisualState x:Name="Focused"/>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="Visibility" Storyboard.TargetName="border">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Normal"/>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="SelectionStates">
                                <VisualState x:Name="Unselected"/>
                                <VisualState x:Name="Selected">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="BorderBrush" Storyboard.TargetName="border">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{telerik:Windows8Resource ResourceKey=AccentBrush}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="Visibility" Storyboard.TargetName="border">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Rectangle x:Name="color" Fill="{TemplateBinding Content}" Stretch="Fill" ToolTipService.ToolTip="{Binding}" MinWidth="13" MinHeight="13" Margin="2 0"/>
                        <Border x:Name="border"
                                BorderBrush="{telerik:Windows8Resource ResourceKey=BasicBrush}"
                                BorderThickness="1"
                                Visibility="Collapsed"
                                VerticalAlignment="Stretch"
                                MinWidth="13"
                                MinHeight="13"
                                Margin="2 0"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Content" Value="Color"/>
        <Setter Property="Color" Value="Green"/>
    </Style>
    <Style x:Key="DefaultStylePredefined" BasedOn="{StaticResource RadColorPaletteViewItemStylePredefined}" TargetType="telerik:RadColorPaletteViewItem">
        <Setter Property="Color" Value="Black"/>
    </Style>
    <Style x:Key="RadColorSelectorStylePredefined" TargetType="telerik:RadColorSelector">
        <Setter Property="Background" Value="{telerik:Windows8Resource ResourceKey=MainBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:Windows8Resource ResourceKey=BasicBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="FontFamily" Value="{telerik:Windows8Resource ResourceKey=FontFamily}"/>
        <Setter Property="Width" Value="170"/>
        <Setter Property="Foreground" Value="{telerik:Windows8Resource ResourceKey=MarkerBrush}"/>
        <Setter Property="FontStyle" Value="Normal"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadColorSelector">
                    <Border BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}">
                        <Grid x:Name="RootElement" Background="{TemplateBinding Background}" Margin="0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Button x:Name="PaletteHeader"
                                    Content="{TemplateBinding NoColorText}"
                                    Tag="{Binding AutomaticColor, Converter={StaticResource converter}, RelativeSource={RelativeSource TemplatedParent}}"
                                    Width="0"
                                    Height="0"
                                    Visibility="Collapsed"/>
                            <telerik:RadColorPaletteView x:Name="HeaderPalette"
                                    BorderBrush="Transparent"
                                    BorderThickness="0"
                                    Background="{TemplateBinding Background}"
                                    Margin="0 0 0 -3"
                                    Grid.Row="2"
                                    VerticalAlignment="Center">
                                <telerik:RadColorPaletteView.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <telerik:RadUniformGrid Background="Transparent" Columns="10"/>
                                    </ItemsPanelTemplate>
                                </telerik:RadColorPaletteView.ItemsPanel>
                            </telerik:RadColorPaletteView>
                            <telerik:RadColorPaletteView x:Name="MainPalette"
                                    BorderBrush="Transparent"
                                    BorderThickness="0"
                                    Background="{TemplateBinding Background}"
                                    ItemContainerStyle="{TemplateBinding MainPaletteItemsStyle}"
                                    Margin="0 5 0 2"
                                    Grid.Row="3"/>
                            <Border x:Name="StandardPaletteBorder" Margin="0 2 0 3" Grid.Row="4" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" Height="0"/>
                            <telerik:RadColorPaletteView x:Name="StandartPalette"
                                    BorderBrush="Transparent"
                                    BorderThickness="0"
                                    Background="{TemplateBinding Background}"
                                    Margin="0 0 0 1"
                                    Grid.Row="5"
                                    VerticalAlignment="Center">
                                <telerik:RadColorPaletteView.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <telerik:RadUniformGrid Background="Transparent" Columns="10"/>
                                    </ItemsPanelTemplate>
                                </telerik:RadColorPaletteView.ItemsPanel>
                            </telerik:RadColorPaletteView>
                            <Border Margin="0 2 0 3" x:Name="RecentColorsPaletteBorder" Grid.Row="6" Visibility="Collapsed" BorderThickness="0 0 0 1">
                                <TextBlock
                                        Margin="10 2 2 2"
                                        x:Name="RecentColorsPaletteHeader"
                                        FontWeight="{TemplateBinding FontWeight}"
                                        Foreground="{TemplateBinding Foreground}"
                                        Text="{TemplateBinding RecentColorsHeaderText}"/>
                            </Border>
                            <telerik:RadColorPaletteView
                                    Grid.Row="7"
                                    x:Name="RecentColorsPalette"
                                    Margin="0 0 0 1"
                                    BorderThickness="0"
                                    Background="Transparent"
                                    BorderBrush="Transparent"
                                    Visibility="Collapsed"
                                    ColorPropertyPath="{TemplateBinding ColorPropertyPath}"
                                    VerticalAlignment="Center"
                                    MinHeight="17">
                                <telerik:RadColorPaletteView.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <telerik:RadUniformGrid Background="Transparent" Columns="10" Rows="1"/>
                                    </ItemsPanelTemplate>
                                </telerik:RadColorPaletteView.ItemsPanel>
                            </telerik:RadColorPaletteView>
                            <Border Margin="0 1 0 0" Visibility="Collapsed" x:Name="AdditionalContentBorder" Grid.Row="8" BorderBrush="Transparent" BorderThickness="0">
                                <ContentPresenter Margin="0 1 0 0" Content="{TemplateBinding AdditionalContent}"/>
                            </Border>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="ColorSelectorDropDownStyle" TargetType="spreadsheetControls:ColorSelectorDropDown">
        <Setter Property="BorderBrush" Value="{telerik:Windows8Resource ResourceKey=BasicBrush}"/>
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="Height" Value="23"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="UseLayoutRounding" Value="True"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="spreadsheetControls:ColorSelectorDropDown">
                    <telerik:RadDropDownButton x:Name="ColorButton" IsTabStop="False" HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}" Padding="{TemplateBinding Padding}">
                        <telerik:RadDropDownButton.Content>
                            <Grid x:Name="ContentContainer">
                                <Border x:Name="ColorRectangleBorder" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}" Margin="{TemplateBinding Padding}">
                                    <Rectangle x:Name="ColorRectangle" Stretch="Fill" Width="500" Height="500">
                                        <Rectangle.Fill>
                                            <SolidColorBrush Color="{Binding ElementName=ColorSelector, Path=SelectedColor}"/>
                                        </Rectangle.Fill>
                                    </Rectangle>
                                </Border>
                            </Grid>
                        </telerik:RadDropDownButton.Content>
                        <telerik:RadDropDownButton.DropDownContent>
                            <StackPanel>
                                <telerik:RadColorSelector x:Name="ColorSelector"
                                        AutomaticColor="{Binding Path=AutomaticColor, RelativeSource={RelativeSource TemplatedParent}}"
                                        SelectedColor="Black"
                                        IsTabStop="False"
                                        HeaderPaletteItemsSource="{Binding Path=HeaderPaletteItemsSource, RelativeSource={RelativeSource TemplatedParent}}"
                                        MainPaletteItemsSource="{Binding Path=MainPaletteItemsSource, RelativeSource={RelativeSource TemplatedParent}}"
                                        MainPaletteOrientation="{Binding Path=MainPaletteOrientation, RelativeSource={RelativeSource TemplatedParent}}"
                                        BorderThickness="0"/>
                                <telerik:RadButton x:Name="MoreColorsButton"
                                        HorizontalContentAlignment="Left"
                                        BorderThickness="0 1 0 0"
                                        Background="{Binding Background,ElementName=ColorSelector}"
                                        Grid.Row="6"
                                        IsTabStop="False">
                                    <StackPanel Orientation="Horizontal">
                                        <Image Source="/Telerik.Windows.Controls.Spreadsheet;component/Images/Light/16/color.png" Width="16" Height="16"/>
                                        <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_ColorSelector_MoreColors" Margin="10 0 0 0" Foreground="{TemplateBinding Foreground}"/>
                                    </StackPanel>
                                </telerik:RadButton>
                            </StackPanel>
                        </telerik:RadDropDownButton.DropDownContent>
                    </telerik:RadDropDownButton>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadHexagonColorPickerStyle" TargetType="spreadsheetControls:RadHexagonColorPicker">
        <Setter Property="UseLayoutRounding" Value="True"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="spreadsheetControls:RadHexagonColorPicker">
                    <Canvas x:Name="PART_FullColorCanvas" Width="220" Height="253" Background="{telerik:Windows8Resource ResourceKey=MainBrush}"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="HexagonItemStyle" TargetType="spreadsheetControls:HexagonItem">
        <Setter Property="UseLayoutRounding" Value="True"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="spreadsheetControls:HexagonItem">
                    <Grid>
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualStateGroup.Transitions>
                                    <VisualTransition GeneratedDuration="0:0:0.1">
                                        <VisualTransition.GeneratedEasingFunction>
                                            <CircleEase EasingMode="EaseOut"/>
                                        </VisualTransition.GeneratedEasingFunction>
                                    </VisualTransition>
                                </VisualStateGroup.Transitions>
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleY)" Storyboard.TargetName="PART_Polygon">
                                            <EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="1.35"/>
                                        </DoubleAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)" Storyboard.TargetName="PART_Polygon">
                                            <EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="1.35"/>
                                        </DoubleAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Initial"/>
                                <VisualState x:Name="Pressed">
                                    <Storyboard>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleY)" Storyboard.TargetName="PART_Polygon">
                                            <EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="1.4"/>
                                        </DoubleAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)" Storyboard.TargetName="PART_Polygon">
                                            <EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="1.4"/>
                                        </DoubleAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Disabled"/>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Polygon x:Name="PART_Polygon" RenderTransformOrigin="0.92, 0.92">
                            <Polygon.RenderTransform>
                                <TransformGroup>
                                    <ScaleTransform/>
                                    <SkewTransform/>
                                    <RotateTransform/>
                                    <TranslateTransform/>
                                </TransformGroup>
                            </Polygon.RenderTransform>
                        </Polygon>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="telerikDragDrop:DragDropManager.AllowDrag" Value="True"/>
        <Setter Property="AllowDrop" Value="True"/>
    </Style>
    <Style TargetType="spreadsheetControls:ColorSelectorDropDown" BasedOn="{StaticResource ColorSelectorDropDownStyle}"/>
    <Style TargetType="spreadsheetControls:RadHexagonColorPicker" BasedOn="{StaticResource RadHexagonColorPickerStyle}"/>
    <Style TargetType="spreadsheetControls:HexagonItem" BasedOn="{StaticResource HexagonItemStyle}"/>
    <Style x:Key="GalleryHeaderItemBaseStyle" TargetType="ContentControl">
        <Setter Property="FontSize" Value="{telerik:Windows8Resource ResourceKey=FontSize}"/>
        <Setter Property="FontFamily" Value="{telerik:Windows8Resource ResourceKey=FontFamily}"/>
        <Setter Property="Padding" Value="5 2"/>
        <Setter Property="Background" Value="{telerik:Windows8Resource ResourceKey=AccentBrush}"/>
        <Setter Property="Foreground" Value="{telerik:Windows8Resource ResourceKey=MainBrush}"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
        <Setter Property="IsHitTestVisible" Value="False"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ContentControl">
                    <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
                        <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" Margin="{TemplateBinding Padding}"/>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="StyleGalleryHeaderItemStyle" TargetType="ListBoxItem" BasedOn="{StaticResource GalleryHeaderItemBaseStyle}">
        <Setter Property="Width" Value="{Binding PopupContentWidth}"/>
    </Style>
    <Style x:Key="GalleryHeaderItemStyle" TargetType="telerik:RadListBoxItem" BasedOn="{StaticResource GalleryHeaderItemBaseStyle}"/>
    <Style x:Key="PatternFillControlStyle" TargetType="spreadsheetControls:PatternFillControl">
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
        <Setter Property="VerticalContentAlignment" Value="Stretch"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="spreadsheetControls:PatternFillControl">
                    <Canvas x:Name="Container" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="GradientFillControlStyle" TargetType="spreadsheetControls:GradientFillControl">
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
        <Setter Property="VerticalContentAlignment" Value="Stretch"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="spreadsheetControls:GradientFillControl">
                    <Canvas x:Name="Container" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="SpreadsheetGridSplitterStyle" TargetType="spreadsheetControls:SpreadsheetGridSplitter">
        <Setter Property="Background" Value="{telerik:Windows8Resource ResourceKey=MainBrush}"/>
        <Setter Property="Foreground" Value="{telerik:Windows8Resource ResourceKey=StrongBrush}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="UseLayoutRounding" Value="True"/>
        <Setter Property="Cursor" Value="SizeWE"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="spreadsheetControls:SpreadsheetGridSplitter">
                    <Grid>
                        <worksheetMargins:ResizeThumb x:Name="Thumb"
                                Width="{TemplateBinding Width}"
                                Height="{TemplateBinding Height}"
                                Foreground="{TemplateBinding Foreground}"
                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                Padding="{TemplateBinding Padding}">
                            <worksheetMargins:ResizeThumb.Template>
                                <ControlTemplate TargetType="worksheetMargins:ResizeThumb">
                                    <Grid>
                                        <Thumb x:Name="PART_Thumb"
                                                Foreground="{TemplateBinding Foreground}"
                                                Background="{TemplateBinding Background}"
                                                BorderBrush="{TemplateBinding BorderBrush}"
                                                BorderThickness="{TemplateBinding BorderThickness}"
                                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                                HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                                Padding="{TemplateBinding Padding}">
                                            <Thumb.Template>
                                                <ControlTemplate>
                                                    <Border
                                                            Background="{Binding Background,RelativeSource={RelativeSource TemplatedParent}}"
                                                            BorderBrush="{Binding BorderBrush,RelativeSource={RelativeSource TemplatedParent}}"
                                                            BorderThickness="{Binding BorderThickness,RelativeSource={RelativeSource TemplatedParent}}"
                                                            Height="{Binding Height,RelativeSource={RelativeSource TemplatedParent}}"
                                                            Width="{Binding Width,RelativeSource={RelativeSource TemplatedParent}}">
                                                        <StackPanel Margin="{Binding Padding,RelativeSource={RelativeSource TemplatedParent}}" VerticalAlignment="{Binding VerticalContentAlignment,RelativeSource={RelativeSource TemplatedParent}}" HorizontalAlignment="{Binding HorizontalContentAlignment,RelativeSource={RelativeSource TemplatedParent}}">
                                                            <Rectangle Width="2" Height="2" Fill="{Binding Foreground,RelativeSource={RelativeSource TemplatedParent}}"/>
                                                            <Rectangle Width="2" Height="2" Fill="{Binding Foreground,RelativeSource={RelativeSource TemplatedParent}}" Margin="0 2"/>
                                                            <Rectangle Width="2" Height="2" Fill="{Binding Foreground,RelativeSource={RelativeSource TemplatedParent}}"/>
                                                        </StackPanel>
                                                    </Border>
                                                </ControlTemplate>
                                            </Thumb.Template>
                                        </Thumb>
                                    </Grid>
                                </ControlTemplate>
                            </worksheetMargins:ResizeThumb.Template>
                        </worksheetMargins:ResizeThumb>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadSpreadsheetStyle" TargetType="telerikControls:RadSpreadsheet">
        <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
        <Setter Property="VerticalContentAlignment" Value="Stretch"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="Background" Value="{telerik:Windows8Resource ResourceKey=MainBrush}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikControls:RadSpreadsheet">
                    <Grid x:Name="LayoutRoot" Background="{TemplateBinding Background}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <ContentControl x:Name="PART_PrintContentContainer"
                                    Opacity="0"
                                    HorizontalAlignment="Stretch"
                                    VerticalAlignment="Stretch"
                                    HorizontalContentAlignment="Stretch"
                                    VerticalContentAlignment="Stretch"
                                    IsTabStop="False"
                                    Focusable="False"/>
                            <ContentControl x:Name="ContentPresenter"
                                    HorizontalAlignment="Stretch"
                                    VerticalAlignment="Stretch"
                                    HorizontalContentAlignment="Stretch"
                                    VerticalContentAlignment="Stretch"
                                    IsTabStop="False"
                                    Focusable="False"/>
                            <Grid Grid.Row="1" Grid.Column="0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="6*"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="4*"/>
                                </Grid.ColumnDefinitions>
                                <spreadsheetControls:RadSpreadsheetSheetSelector x:Name="SheetSelector" RadSpreadsheet="{Binding RelativeSource={RelativeSource TemplatedParent}}" Height="27" AreSheetTabItemsOverlapping="False"/>
                                <Rectangle Height="1" VerticalAlignment="Top" Fill="{telerik:Windows8Resource ResourceKey=BasicBrush}" Grid.Column="1" Grid.ColumnSpan="2"/>
                                <spreadsheetControls:SpreadsheetGridSplitter x:Name="splitter"
                                        Width="10"
                                        VerticalAlignment="Stretch"
                                        HorizontalAlignment="Center"
                                        Visibility="{Binding Path=(spreadsheetControls:RadSpreadsheetSheetSelector.IsSheetSelectorVisible), RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource SpreadsheetBooleanToVisibilityConverter}}"
                                        Margin="0 1 0 0"
                                        Grid.Column="1"/>
                                <ScrollBar x:Name="HorizontalScrollBar"
                                        Grid.Column="2"
                                        HorizontalAlignment="Stretch"
                                        IsTabStop="False"
                                        SmallChange="16"
                                        LargeChange="320"
                                        Orientation="Horizontal"
                                        Minimum="0"
                                        Margin="{StaticResource HorizontalScrollBarMarginThickness}"
                                        Height="12"
                                        VerticalAlignment="Center"/>
                            </Grid>
                            <ScrollBar x:Name="VerticalScrollBar" Grid.Row="0" Grid.Column="1" IsTabStop="False" SmallChange="16" LargeChange="320" Orientation="Vertical" Minimum="0" Width="12"/>
                            <Rectangle Width="1" Fill="{telerik:Windows8Resource ResourceKey=BasicBrush}" Grid.Column="1" HorizontalAlignment="Left"/>
                            <Rectangle Grid.Row="1" Grid.Column="1" Fill="{telerik:Windows8Resource ResourceKey=MainBrush}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="NormalWorksheetEditorPresenterStyle" TargetType="worksheet:NormalWorksheetEditorPresenter">
