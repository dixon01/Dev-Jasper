        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="dialogs:CustomFilterDialogContent">
                    <Grid
                            Margin="{TemplateBinding Padding}"
                            Background="{TemplateBinding Background}"
                            HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                            VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_CustomFilterDialog_ShowRowsWhere" Foreground="{TemplateBinding Foreground}"/>
                        <telerik:RadComboBox
                                Grid.Row="1"
                                HorizontalAlignment="Stretch"
                                ItemsSource="{Binding Path=ComparisonOperatorItemsSource}"
                                SelectedValue="{Binding Path=SelectedComparisonOperator1, Mode=TwoWay}">
                            <telerik:RadComboBox.ItemTemplate>
                                <DataTemplate>
                                    <TextBlock Text="{Binding Converter={StaticResource ComparisonOperatorItemToStringConverter}}"/>
                                </DataTemplate>
                            </telerik:RadComboBox.ItemTemplate>
                        </telerik:RadComboBox>
                        <TextBox Grid.Row="1" Grid.Column="1" Text="{Binding Path=Value1, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="5 0"/>
                        <StackPanel Orientation="Horizontal" Grid.Row="2" Grid.ColumnSpan="2" Margin="5">
                            <RadioButton IsChecked="{Binding Path=SelectedLogicalOperator, Mode=TwoWay, Converter={StaticResource LogicalOperatorToBooleanConverter}, ConverterParameter=And}" Margin="5 3" telerik:LocalizationManager.ResourceKey="Spreadsheet_CustomFilterDialog_And"/>
                            <RadioButton IsChecked="{Binding Path=SelectedLogicalOperator, Mode=TwoWay, Converter={StaticResource LogicalOperatorToBooleanConverter}, ConverterParameter=Or}" Margin="5 3" telerik:LocalizationManager.ResourceKey="Spreadsheet_CustomFilterDialog_Or"/>
                        </StackPanel>
                        <telerik:RadComboBox Grid.Row="3" ItemsSource="{Binding Path=ComparisonOperatorItemsSource}" SelectedValue="{Binding Path=SelectedComparisonOperator2, Mode=TwoWay}">
                            <telerik:RadComboBox.ItemTemplate>
                                <DataTemplate>
                                    <TextBlock Text="{Binding Converter={StaticResource ComparisonOperatorItemToStringConverter}}"/>
                                </DataTemplate>
                            </telerik:RadComboBox.ItemTemplate>
                        </telerik:RadComboBox>
                        <TextBox Grid.Row="3" Grid.Column="1" Text="{Binding Path=Value2, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="5 0"/>
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Grid.Row="4" Grid.ColumnSpan="2" VerticalAlignment="Bottom" Margin="5 0">
                            <telerik:RadButton telerik:LocalizationManager.ResourceKey="Ok" Margin="10 0 0 0" x:Name="PART_ButtonOK" Command="{Binding Path=OKCommand}" MinWidth="75"/>
                            <telerik:RadButton telerik:LocalizationManager.ResourceKey="Cancel" Margin="10 0 0 0" x:Name="PART_ButtonCancel" Command="{Binding Path=CancelCommand}" MinWidth="75"/>
                        </StackPanel>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
