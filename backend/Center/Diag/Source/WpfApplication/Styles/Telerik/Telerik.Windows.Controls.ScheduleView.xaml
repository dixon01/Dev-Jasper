        <telerikScheduleView:Category CategoryName="Purple Category" CategoryBrush="{StaticResource PurpleCategoryBrush}"/>
        <telerikScheduleView:Category CategoryName="Yellow Category" CategoryBrush="{StaticResource YellowCategoryBrush}"/>
        <telerikScheduleView:Category CategoryName="Olive Category" CategoryBrush="{StaticResource OliveCategoryBrush}"/>
