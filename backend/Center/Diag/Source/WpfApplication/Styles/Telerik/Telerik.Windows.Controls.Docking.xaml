                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed" />
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Path Name="PathVisual" Data="M0,2L6,2 3,6z M0,0L6,0 6,1 0,1z" Stretch="None" HorizontalAlignment="Center" VerticalAlignment="Center" SnapsToDevicePixels="True" Fill="{wrd:Windows8Resource ResourceKey=StrongBrush}" />
            <Popup Name="DropDownPopup" AllowsTransparency="True" StaysOpen="False" Focusable="False" Placement="Bottom">
                <Grid>
                    <Border Name="DropDownPopupBorder" MinWidth="3" MinHeight="3" Width="{TemplateBinding wrd:RadDropDownButton.DropDownWidth}" Height="{TemplateBinding wrd:RadDropDownButton.DropDownHeight}" MaxWidth="{TemplateBinding wrd:RadDropDownButton.DropDownMaxWidth}" MaxHeight="{TemplateBinding wrd:RadDropDownButton.DropDownMaxHeight}" Background="{wrd:Windows8Resource ResourceKey=MainBrush}" BorderBrush="{wrd:Windows8Resource ResourceKey=BasicBrush}">
                        <ContentControl Name="DropDownPopupContent" Content="{TemplateBinding wrd:RadDropDownButton.DropDownContent}" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" ContentTemplate="{TemplateBinding wrd:RadDropDownButton.DropDownContentTemplate}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" ContentTemplateSelector="{TemplateBinding ContentControl.ContentTemplateSelector}" ContentStringFormat="{TemplateBinding ContentControl.ContentStringFormat}" DataContext="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=DataContext}" />
                    </Border>
                </Grid>
            </Popup>
        </Grid>
    </ControlTemplate>
    <Style x:Key="DropDownButtonDocumentDropDownStyle" TargetType="{x:Type wrd:RadDropDownButton}">
        <Setter Property="Template" Value="{StaticResource DropDownButtonDocumentDropDownControlTemplate}" />
