            <Popup Grid.Row="0" Name="ContentPopup" IsOpen="False" AllowsTransparency="True" Placement="Top">
                <Grid>
                    <Grid Name="PopupChild" MinWidth="50" MinHeight="50">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>
                        <ph:RadGridResizer x:Name="Resizer" Grid.Row="0" Height="4" wrd:StyleManager.Theme="{StaticResource Theme}" Placement="Top" HorizontalAlignment="Stretch" VerticalAlignment="Top" Background="#01010101" Cursor="SizeNS" />
                        <ContentControl Name="ContentBackground" Grid.Row="1" BorderBrush="{TemplateBinding Control.BorderBrush}" BorderThickness="{TemplateBinding Control.BorderThickness}" Background="{TemplateBinding Control.Background}" Template="{StaticResource PopupContentTemplate}">
                            <Grid>
