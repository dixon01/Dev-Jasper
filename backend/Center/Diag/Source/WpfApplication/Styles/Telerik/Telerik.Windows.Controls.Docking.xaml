                        </Grid.ColumnDefinitions>
                        <ContentControl Name="ContentBackground" Grid.Column="0" BorderBrush="{TemplateBinding Control.BorderBrush}" BorderThickness="{TemplateBinding Control.BorderThickness}" Background="{TemplateBinding Control.Background}" Template="{StaticResource PopupContentTemplate}">
                            <Grid>
