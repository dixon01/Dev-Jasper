    <ControlTemplate x:Key="AutoHideAreaTopTemplate" TargetType="{x:Type ph:AutoHideArea}">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <ItemsPresenter Name="ItemsPresenterElement" />
            <Popup Name="ContentPopup" Grid.Row="1" IsOpen="False" AllowsTransparency="True">
                <Grid>
                    <Grid Name="PopupChild" MinWidth="50" MinHeight="50">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*" />
                            <RowDefinition Height="Auto" />
                        </Grid.RowDefinitions>
                        <ContentControl Name="ContentBackground" Grid.Row="0" Template="{StaticResource PopupContentTemplate}" BorderBrush="{TemplateBinding Control.BorderBrush}" BorderThickness="{TemplateBinding Control.BorderThickness}" Background="{TemplateBinding Control.Background}">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                <ph:PaneHeader x:Name="HeaderElement" Grid.Row="0" SelectedPane="{TemplateBinding ph:PaneGroupBase.SelectedPane}" wrd:StyleManager.Theme="{StaticResource Theme}" MinHeight="16" Visibility="{Binding SelectedPane.PaneHeaderVisibility, RelativeSource={RelativeSource TemplatedParent}}" />
                                <ContentPresenter Name="ContentElement" Grid.Row="1" ContentTemplate="{TemplateBinding ddm:RadTabControlBase.SelectedContentTemplate}" />
                            </Grid>
                        </ContentControl>
                        <ph:RadGridResizer x:Name="Resizer" wrd:StyleManager.Theme="{StaticResource Theme}" Background="#01010101" Placement="Bottom" HorizontalAlignment="Stretch" VerticalAlignment="Bottom" Grid.Row="1" Height="4" Cursor="SizeNS" />
                    </Grid>
                </Grid>
            </Popup>
        </Grid>
    </ControlTemplate>
    <ControlTemplate x:Key="AutoHideAreaBottomTemplate" TargetType="{x:Type ph:AutoHideArea}">
