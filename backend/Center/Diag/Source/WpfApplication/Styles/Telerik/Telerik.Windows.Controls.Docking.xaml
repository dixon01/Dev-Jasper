            </Path>
        </Grid>
    </ControlTemplate>
    <Style x:Key="RadButtonPinStyle" TargetType="{x:Type wrd:RadButton}">
        <Setter Property="Template" Value="{StaticResource RadButtonPinControlTemplate}" />
        <Setter Property="Foreground" Value="{wrd:Windows8Resource ResourceKey=MainBrush}" />
