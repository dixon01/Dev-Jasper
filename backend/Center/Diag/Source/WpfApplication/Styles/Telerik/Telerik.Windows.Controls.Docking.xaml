                    </VisualState>
                    <VisualState x:Name="Pressed" />
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Path Fill="{TemplateBinding Control.Foreground}" Stretch="None" StrokeThickness="0" Width="10" Height="8" RenderTransformOrigin="0.5 0.5" Data="M4,2C4,2 4,4 4,4 4,4 8.2,4 9,4L9,3C9,2.3,8.3,2,7.5,2z M3,0L4,0 4,1 8,1C9.1,1,10,1.8,10,3L10,4C10,5,9,6,8,6L4,6 4,7 3,7 3,4 0,4 0,3 3,3z">
