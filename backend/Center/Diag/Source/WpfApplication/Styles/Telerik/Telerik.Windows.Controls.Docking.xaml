                </VisualStateGroup>
                <VisualStateGroup x:Name="TopIndicatorVisibilityStates">
                    <VisualState x:Name="TopIndicatorVisibile" />
                    <VisualState x:Name="TopIndicatorHidden">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_TopIndicator" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="RightIndicatorVisibilityStates">
                    <VisualState x:Name="RightIndicatorVisibile" />
                    <VisualState x:Name="RightIndicatorHidden">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_RightIndicator" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="BottomIndicatorVisibilityStates">
                    <VisualState x:Name="BottomIndicatorVisibile" />
                    <VisualState x:Name="BottomIndicatorHidden">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_BottomIndicator" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="CenterIndicatorVisibilityStates">
                    <VisualState x:Name="CenterIndicatorVisibile" />
                    <VisualState x:Name="CenterIndicatorHidden">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_CenterIndicator" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border.Clip>
                <EllipseGeometry Center="24.5 24.5" RadiusX="24.5" RadiusY="24.5" />
            </Border.Clip>
            <Grid Width="49" Height="49">
                <Grid Width="45" Height="45" Background="#00FFFFFF">
                    <Grid.Clip>
                        <GeometryGroup FillRule="EvenOdd">
                            <EllipseGeometry Center="22.5 22.5" RadiusX="22.5" RadiusY="22.5" />
                            <EllipseGeometry Center="22.5 22.5" RadiusX="10" RadiusY="10" />
                        </GeometryGroup>
                    </Grid.Clip>
                    <Grid Name="PART_LeftIndicator">
                        <Path Name="LeftHighlightVisual" Opacity="0" Data="M0,0L22.5,22.5 0,45z" Fill="{wrd:Windows8Resource ResourceKey=BasicBrush}" />
                        <Path Width="9" Height="9" Margin="2" HorizontalAlignment="Left" Data="M9,0L0,4.5 9,9z" Fill="{wrd:Windows8Resource ResourceKey=StrongBrush}" />
                    </Grid>
                    <Grid Name="PART_TopIndicator">
                        <Path Name="TopHighlightVisual" Opacity="0" Data="M0,0L22.5,22.5 45,0z" Fill="{wrd:Windows8Resource ResourceKey=BasicBrush}" />
                        <Path Width="9" Height="9" Margin="2" VerticalAlignment="Top" Data="M0,9L4.5,0 9,9z" Fill="{wrd:Windows8Resource ResourceKey=StrongBrush}" />
                    </Grid>
                    <Grid Name="PART_RightIndicator">
                        <Path Name="RightHighlightVisual" Opacity="0" Data="M45,0L22.5,22.5 45,45z" Fill="{wrd:Windows8Resource ResourceKey=BasicBrush}" />
                        <Path Width="9" Height="9" Margin="2" HorizontalAlignment="Right" Data="M0,0L9,4.5 0,9z" Fill="{wrd:Windows8Resource ResourceKey=StrongBrush}" />
                    </Grid>
                    <Grid Name="PART_BottomIndicator">
                        <Path Name="BottomHighlightVisual" Opacity="0" Data="M0,45L22.5,22.5 45,45z" Fill="{wrd:Windows8Resource ResourceKey=BasicBrush}" />
                        <Path Width="9" Height="9" Margin="2" VerticalAlignment="Bottom" Data="M0,0L4.5,9 9,0z" Fill="{wrd:Windows8Resource ResourceKey=StrongBrush}" />
                    </Grid>
                </Grid>
                <Grid Name="PART_CenterIndicator" Width="21" Height="21">
                    <Ellipse Name="CenterHighlightVisual" Opacity="0" Fill="{wrd:Windows8Resource ResourceKey=BasicBrush}" />
                    <Ellipse Width="5" Height="5" Fill="{wrd:Windows8Resource ResourceKey=StrongBrush}" />
                </Grid>
                <Ellipse StrokeThickness="2" Stroke="{wrd:Windows8Resource ResourceKey=BasicBrush}" />
            </Grid>
        </Border>
    </ControlTemplate>
    <Style x:Key="CompassStyle" TargetType="{x:Type ph:Compass}">
        <Setter Property="Background" Value="{wrd:Windows8Resource ResourceKey=MainBrush}" />
        <Setter Property="BorderBrush" Value="{wrd:Windows8Resource ResourceKey=BasicBrush}" />
        <Setter Property="Template" Value="{StaticResource CompassControlTemplate}" />
    </Style>
    <ControlTemplate x:Key="RootCompassControlTemplate" TargetType="{x:Type ph:Compass}">
        <Grid Name="VisualRoot">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal" />
                    <VisualState x:Name="HighlightLeftIndicator">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_LeftIndicator" Storyboard.TargetProperty="Background">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{wrd:Windows8Resource ResourceKey=BasicBrush}" />
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="HighlightTopIndicator">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_TopIndicator" Storyboard.TargetProperty="Background">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{wrd:Windows8Resource ResourceKey=BasicBrush}" />
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="HighlightRightIndicator">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_RightIndicator" Storyboard.TargetProperty="Background">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{wrd:Windows8Resource ResourceKey=BasicBrush}" />
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="HighlightBottomIndicator">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_BottomIndicator" Storyboard.TargetProperty="Background">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{wrd:Windows8Resource ResourceKey=BasicBrush}" />
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="LeftIndicatorVisibilityStates">
                    <VisualState x:Name="LeftIndicatorVisibile" />
                    <VisualState x:Name="LeftIndicatorHidden">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_LeftIndicator" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="TopIndicatorVisibilityStates">
                    <VisualState x:Name="TopIndicatorVisibile" />
                    <VisualState x:Name="TopIndicatorHidden">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_TopIndicator" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="RightIndicatorVisibilityStates">
                    <VisualState x:Name="RightIndicatorVisibile" />
                    <VisualState x:Name="RightIndicatorHidden">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_RightIndicator" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="BottomIndicatorVisibilityStates">
                    <VisualState x:Name="BottomIndicatorVisibile" />
                    <VisualState x:Name="BottomIndicatorHidden">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_BottomIndicator" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border Name="PART_LeftIndicator" Margin="4" Width="22" Height="22" HorizontalAlignment="Left" CornerRadius="11" BorderThickness="1" Background="{wrd:Windows8Resource ResourceKey=MainBrush}" BorderBrush="{wrd:Windows8Resource ResourceKey=BasicBrush}">
                <Path Width="5" Height="9" Data="M5,0L0,4.5 5,9z" Fill="{wrd:Windows8Resource ResourceKey=StrongBrush}" />
            </Border>
            <Border Name="PART_TopIndicator" Margin="4" Width="22" Height="22" VerticalAlignment="Top" CornerRadius="11" BorderThickness="1" Background="{wrd:Windows8Resource ResourceKey=MainBrush}" BorderBrush="{wrd:Windows8Resource ResourceKey=BasicBrush}">
                <Path Width="9" Height="5" Data="M0,5L4.5,0 9,5z" Fill="{wrd:Windows8Resource ResourceKey=StrongBrush}" />
            </Border>
            <Border Name="PART_RightIndicator" Margin="4" Width="22" Height="22" HorizontalAlignment="Right" CornerRadius="11" BorderThickness="1" Background="{wrd:Windows8Resource ResourceKey=MainBrush}" BorderBrush="{wrd:Windows8Resource ResourceKey=BasicBrush}">
                <Path Width="5" Height="9" Data="M0,0L5,4.5 0,9z" Fill="{wrd:Windows8Resource ResourceKey=StrongBrush}" />
            </Border>
            <Border Name="PART_BottomIndicator" Margin="4" Width="22" Height="22" VerticalAlignment="Bottom" CornerRadius="11" BorderThickness="1" Background="{wrd:Windows8Resource ResourceKey=MainBrush}" BorderBrush="{wrd:Windows8Resource ResourceKey=BasicBrush}">
                <Path Width="9" Height="5" Data="M0,0L4.5,5 9,0z" Fill="{wrd:Windows8Resource ResourceKey=StrongBrush}" />
            </Border>
        </Grid>
    </ControlTemplate>
    <Style x:Key="RootCompassStyle" TargetType="{x:Type ph:RootCompass}">
        <Setter Property="Template" Value="{StaticResource RootCompassControlTemplate}" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="Panel.ZIndex" Value="100" />
    </Style>
    <ControlTemplate x:Key="VisualCueControlTemplate" TargetType="{x:Type ph:VisualCue}">
        <Border Background="{TemplateBinding Control.Background}" BorderBrush="{TemplateBinding Control.BorderBrush}" BorderThickness="{TemplateBinding Control.BorderThickness}" />
    </ControlTemplate>
    <Style x:Key="VisualCueStyle" TargetType="{x:Type ph:VisualCue}">
        <Setter Property="Background" Value="{wrd:Windows8Resource ResourceKey=BasicBrush}" />
        <Setter Property="Opacity" Value="0.5" />
        <Setter Property="BorderBrush" Value="{wrd:Windows8Resource ResourceKey=StrongBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="Template" Value="{StaticResource VisualCueControlTemplate}" />
    </Style>
    <Style x:Key="DocumentHostStyle" TargetType="{x:Type ph:DocumentHost}">
        <Setter Property="HorizontalContentAlignment" Value="Stretch" />
        <Setter Property="VerticalContentAlignment" Value="Stretch" />
        <Setter Property="MinWidth" Value="25" />
        <Setter Property="MinHeight" Value="25" />
        <Setter Property="Background" Value="#00FFFFFF" />
        <Setter Property="AllowDrop" Value="True" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ph:DocumentHost}">
                    <Border Background="{TemplateBinding Control.Background}" BorderBrush="{TemplateBinding Control.BorderBrush}" BorderThickness="{TemplateBinding Control.BorderThickness}" Padding="{TemplateBinding Control.Padding}">
                        <ContentPresenter ContentTemplate="{TemplateBinding ContentControl.ContentTemplate}" Content="{TemplateBinding ContentControl.Content}" HorizontalAlignment="{TemplateBinding Control.HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding Control.VerticalContentAlignment}" />
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <ControlTemplate x:Key="RadDockingControlTemplate" TargetType="{x:Type psp:RadDocking}">
        <Grid Name="Root">
            <Border Name="Border" Background="{TemplateBinding Control.Background}" BorderBrush="{TemplateBinding Control.BorderBrush}" BorderThickness="{TemplateBinding Control.BorderThickness}" Padding="{TemplateBinding Control.Padding}">
                <wrd:RadDockPanel Name="RootDockPanel">
                    <ph:AutoHideArea x:Name="LeftAutoHide" wrd:RadDockPanel.Dock="Left" TabStripPlacement="Left" BackgroundVisibility="Collapsed" TabOrientation="Vertical" wrd:StyleManager.Theme="{StaticResource Theme}" />
                    <ph:AutoHideArea x:Name="RightAutoHide" wrd:RadDockPanel.Dock="Right" TabStripPlacement="Right" BackgroundVisibility="Collapsed" TabOrientation="Vertical" wrd:StyleManager.Theme="{StaticResource Theme}" />
                    <ph:AutoHideArea x:Name="TopAutoHide" wrd:RadDockPanel.Dock="Top" TabStripPlacement="Top" BackgroundVisibility="Collapsed" TabOrientation="Horizontal" wrd:StyleManager.Theme="{StaticResource Theme}" />
                    <ph:AutoHideArea x:Name="BottomAutoHide" wrd:RadDockPanel.Dock="Bottom" TabStripPlacement="Bottom" BackgroundVisibility="Collapsed" TabOrientation="Horizontal" wrd:StyleManager.Theme="{StaticResource Theme}" />
                    <psp:DockingPanel x:Name="ContentDockPanel">
                        <ph:DocumentHost x:Name="DocumentHost" wrd:StyleManager.Theme="{StaticResource Theme}" />
                    </psp:DockingPanel>
                </wrd:RadDockPanel>
            </Border>
        </Grid>
    </ControlTemplate>
    <as:AnimationSelector x:Key="RadDockingAnimationSelector">
        <as:AnimationGroup AnimationName="Move">
            <as:MoveAnimation />
            <as:ResizeAnimation />
            <as:FadeAnimation Direction="In" />
            <as:ScaleAnimation Direction="In" MinScale="0.7" MaxScale="1.0" />
        </as:AnimationGroup>
        <as:AnimationGroup AnimationName="Show">
            <as:FadeAnimation Direction="In" />
            <as:ScaleAnimation Direction="In" MinScale="0.95" MaxScale="1.0" />
        </as:AnimationGroup>
        <as:AnimationGroup AnimationName="Hide" SpeedRatio="0.7">
            <as:FadeAnimation Direction="Out" />
            <as:ScaleAnimation Direction="Out" MinScale="0.7" MaxScale="1.0" />
        </as:AnimationGroup>
        <as:AnimationGroup AnimationName="LeftIn">
            <as:SlideAnimation SlideMode="Top" Duration="{StaticResource AutoHideInAnimationDuration}" TargetElementName="PopupChild" Orientation="Horizontal" Direction="In" />
        </as:AnimationGroup>
        <as:AnimationGroup AnimationName="LeftOut">
            <as:SlideAnimation TargetElementName="PopupChild" SlideMode="Top" Duration="{StaticResource AutoHideOutAnimationDuration}" Orientation="Horizontal" Direction="Out" />
        </as:AnimationGroup>
        <as:AnimationGroup AnimationName="TopIn">
            <as:SlideAnimation Orientation="Vertical" Duration="{StaticResource AutoHideInAnimationDuration}" TargetElementName="PopupChild" Direction="In" />
        </as:AnimationGroup>
        <as:AnimationGroup AnimationName="TopOut">
            <as:SlideAnimation TargetElementName="PopupChild" Duration="{StaticResource AutoHideOutAnimationDuration}" Orientation="Vertical" Direction="Out" />
        </as:AnimationGroup>
        <as:AnimationGroup AnimationName="RightIn">
            <as:SlideAnimation SlideMode="Bottom" TargetElementName="PopupChild" Duration="{StaticResource AutoHideInAnimationDuration}" Orientation="Horizontal" Direction="In" />
        </as:AnimationGroup>
        <as:AnimationGroup AnimationName="RightOut">
            <as:SlideAnimation TargetElementName="PopupChild" SlideMode="Bottom" Duration="{StaticResource AutoHideOutAnimationDuration}" Orientation="Horizontal" Direction="Out" />
        </as:AnimationGroup>
        <as:AnimationGroup AnimationName="BottomIn">
            <as:SlideAnimation SlideMode="Bottom" Duration="{StaticResource AutoHideInAnimationDuration}" Orientation="Vertical" TargetElementName="PopupChild" Direction="In" />
        </as:AnimationGroup>
        <as:AnimationGroup AnimationName="BottomOut">
            <as:SlideAnimation Duration="{StaticResource AutoHideOutAnimationDuration}" TargetElementName="PopupChild" Direction="Out" Orientation="Vertical" SlideMode="Bottom" />
        </as:AnimationGroup>
    </as:AnimationSelector>
    <Style x:Key="RadDockingStyle" TargetType="{x:Type psp:RadDocking}">
        <Setter Property="VisualCueStyle" Value="{StaticResource VisualCueStyle}" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="Padding" Value="0" />
        <Setter Property="Template" Value="{StaticResource RadDockingControlTemplate}" />
        <Setter Property="CompassStyle" Value="{StaticResource CompassStyle}" />
        <Setter Property="RootCompassStyle" Value="{StaticResource RootCompassStyle}" />
        <Setter Property="as:AnimationManager.AnimationSelector" Value="{StaticResource RadDockingAnimationSelector}" />
        <Setter Property="BorderBrush" Value="{wrd:Windows8Resource ResourceKey=BasicBrush}" />
        <Setter Property="Background" Value="{wrd:Windows8Resource ResourceKey=MainBrush}" />
    </Style>
