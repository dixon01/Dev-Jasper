    <Style x:Key="{x:Type psp:RadPaneGroup}" TargetType="{x:Type psp:RadPaneGroup}" BasedOn="{StaticResource RadPaneGroupStyle}" />
    <Style x:Key="{x:Type ph:ToolWindow}" TargetType="{x:Type ph:ToolWindow}" BasedOn="{StaticResource ToolWindowStyle}" />
    <Style x:Key="{x:Type ph:Compass}" TargetType="{x:Type ph:Compass}" BasedOn="{StaticResource CompassStyle}" />
    <Style x:Key="{x:Type ph:RootCompass}" TargetType="{x:Type ph:RootCompass}" BasedOn="{StaticResource RootCompassStyle}" />
    <Style x:Key="{x:Type ph:VisualCue}" TargetType="{x:Type ph:VisualCue}" BasedOn="{StaticResource VisualCueStyle}" />
    <Style x:Key="{x:Type ph:DocumentHost}" TargetType="{x:Type ph:DocumentHost}" BasedOn="{StaticResource DocumentHostStyle}" />
    <Style x:Key="{x:Type psp:RadDocking}" TargetType="{x:Type psp:RadDocking}" BasedOn="{StaticResource RadDockingStyle}" />
    <Style x:Key="{wrd:ThemeResourceKey ThemeType={x:Type wrd:Windows8Theme}, ElementType={x:Type ph:AutoHideArea}}" TargetType="{x:Type ph:AutoHideArea}" BasedOn="{StaticResource AutoHideAreaStyle}" />
    <Style x:Key="{wrd:ThemeResourceKey ThemeType={x:Type wrd:Windows8Theme}, ElementType={x:Type psp:RadPane}}" TargetType="{x:Type psp:RadPane}" BasedOn="{StaticResource RadPaneStyle}" />
    <Style x:Key="{wrd:ThemeResourceKey ThemeType={x:Type wrd:Windows8Theme}, ElementType={x:Type psp:RadDocumentPane}}" TargetType="{x:Type psp:RadDocumentPane}" BasedOn="{StaticResource RadPaneStyle}" />
    <Style x:Key="{wrd:ThemeResourceKey ThemeType={x:Type wrd:Windows8Theme}, ElementType={x:Type ph:RadGridResizer}}" TargetType="{x:Type ph:RadGridResizer}" BasedOn="{StaticResource RadGridResizerStyle}" />
    <Style x:Key="{wrd:ThemeResourceKey ThemeType={x:Type wrd:Windows8Theme}, ElementType={x:Type psp:RadSplitContainer}}" TargetType="{x:Type psp:RadSplitContainer}" BasedOn="{StaticResource RadSplitContainerStyle}" />
    <Style x:Key="{wrd:ThemeResourceKey ThemeType={x:Type wrd:Windows8Theme}, ElementType={x:Type ph:PaneHeader}}" TargetType="{x:Type ph:PaneHeader}" BasedOn="{StaticResource PaneHeaderStyle}" />
    <Style x:Key="{wrd:ThemeResourceKey ThemeType={x:Type wrd:Windows8Theme}, ElementType={x:Type psp:RadPaneGroup}}" TargetType="{x:Type psp:RadPaneGroup}" BasedOn="{StaticResource RadPaneGroupStyle}" />
    <Style x:Key="{wrd:ThemeResourceKey ThemeType={x:Type wrd:Windows8Theme}, ElementType={x:Type ph:ToolWindow}}" TargetType="{x:Type ph:ToolWindow}" BasedOn="{StaticResource ToolWindowStyle}" />
    <Style x:Key="{wrd:ThemeResourceKey ThemeType={x:Type wrd:Windows8Theme}, ElementType={x:Type ph:Compass}}" TargetType="{x:Type ph:Compass}" BasedOn="{StaticResource CompassStyle}" />
    <Style x:Key="{wrd:ThemeResourceKey ThemeType={x:Type wrd:Windows8Theme}, ElementType={x:Type ph:RootCompass}}" TargetType="{x:Type ph:RootCompass}" BasedOn="{StaticResource RootCompassStyle}" />
    <Style x:Key="{wrd:ThemeResourceKey ThemeType={x:Type wrd:Windows8Theme}, ElementType={x:Type ph:VisualCue}}" TargetType="{x:Type ph:VisualCue}" BasedOn="{StaticResource VisualCueStyle}" />
    <Style x:Key="{wrd:ThemeResourceKey ThemeType={x:Type wrd:Windows8Theme}, ElementType={x:Type ph:DocumentHost}}" TargetType="{x:Type ph:DocumentHost}" BasedOn="{StaticResource DocumentHostStyle}" />
    <Style x:Key="{wrd:ThemeResourceKey ThemeType={x:Type wrd:Windows8Theme}, ElementType={x:Type psp:RadDocking}}" TargetType="{x:Type psp:RadDocking}" BasedOn="{StaticResource RadDockingStyle}" />
