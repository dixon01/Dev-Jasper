    <ControlTemplate x:Key="PopupContentTemplate" TargetType="{x:Type ContentControl}">
        <Border Name="Root" BorderThickness="1" Background="{wrd:Windows8Resource ResourceKey=MainBrush}" BorderBrush="{wrd:Windows8Resource ResourceKey=BasicBrush}">
            <ContentPresenter />
        </Border>
