            </VisualStateManager.VisualStateGroups>
            <Path Name="PathVisual" Width="4" Height="7" Data="M4,0L0,3.5 4,7" Fill="{wrd:Windows8Resource ResourceKey=StrongBrush}" />
        </Grid>
    </ControlTemplate>
    <Style x:Key="RepeatButtonScrollLeftStyle" TargetType="{x:Type RepeatButton}">
