                            <DoubleAnimation To="1" Duration="0:0:0" Storyboard.TargetName="BottomHighlightVisual" Storyboard.TargetProperty="Opacity" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="HighlightCenterIndicator">
                        <Storyboard>
                            <DoubleAnimation To="1" Duration="0:0:0" Storyboard.TargetName="CenterHighlightVisual" Storyboard.TargetProperty="Opacity" />
