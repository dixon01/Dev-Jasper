    <wrd:Windows8Theme x:Key="Theme" />
    <ts:TimeSpan x:Key="AutoHideInAnimationDuration">0:0:0.360</ts:TimeSpan>
    <ts:TimeSpan x:Key="AutoHideOutAnimationDuration">0:0:0.400</ts:TimeSpan>
    <wrd:InvertedBooleanConverter x:Key="InvertedBooleanConverter" />
    <wrd:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
    <wrd:InvertedBooleanToVisibilityConverter x:Key="InvertedBooleanToVisibilityConverter" />
    <ControlTemplate x:Key="RadButtonCloseControlTemplate" TargetType="{x:Type wrd:RadButton}">
        <Grid Name="LayoutRoot" Background="#00FFFFFF" MinWidth="16" Height="16" Opacity="0.7">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Disabled">
                        <Storyboard>
                            <DoubleAnimation Duration="0:0:0" To="0.3" Storyboard.TargetName="LayoutRoot" Storyboard.TargetProperty="Opacity" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Normal" />
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <DoubleAnimation Duration="0:0:0" To="1" Storyboard.TargetName="LayoutRoot" Storyboard.TargetProperty="Opacity" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed" />
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Path Width="10" Height="10" Data="M1,1L9,9 M9,1L1,9" StrokeThickness="2" Stroke="{wrd:Windows8Resource ResourceKey=MainBrush}" />
        </Grid>
    </ControlTemplate>
    <Style x:Key="RadButtonCloseStyle" TargetType="{x:Type wrd:RadButton}">
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="Template" Value="{StaticResource RadButtonCloseControlTemplate}" />
    </Style>
