    <ControlTemplate x:Key="RadButtonPinControlTemplate" TargetType="{x:Type wrd:RadButton}">
        <Grid Name="LayoutRoot" Background="#00FFFFFF" Width="16" Height="16" Opacity="0.7">
            <VisualStateManager.VisualStateGroups>
