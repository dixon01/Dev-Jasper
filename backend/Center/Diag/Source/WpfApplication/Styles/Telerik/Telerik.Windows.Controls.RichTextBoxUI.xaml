                    Visibility="Collapsed"
                    Fill="{TemplateBinding Foreground}"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    UseLayoutRounding="True"
                    Stretch="Fill"
                    Width="5"
                    Height="5"
                    SnapsToDevicePixels="True"/>
            <Path x:Name="CenterTab"
                    Data="M3.46 0 L5.46 0 L5.46 3.03 L9 3.03 L9 5.03 L0 5.03 L0 3.03 L3.46 3.03 z"
                    Visibility="Collapsed"
                    Fill="{TemplateBinding Foreground}"
                    Stretch="Fill"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    UseLayoutRounding="True"
                    Width="8"
                    Height="5"
                    SnapsToDevicePixels="True"/>
            <Path x:Name="DecimalTab"
                    Data="M3.07 0.00 L4.85 0.00 L4.85 3.01 L8 3.01 L8 5.00 L0 5.00 L0 3.01 L3.07 3.01 z M6.21 0 L7.99 0 L7.99 1.78 L6.21 1.78 z"
                    Visibility="Collapsed"
                    Fill="{TemplateBinding Foreground}"
                    Width="8"
                    Height="5"
                    UseLayoutRounding="True"
                    Stretch="Fill"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    SnapsToDevicePixels="True"/>
            <Path x:Name="BarTab"
                    Visibility="Collapsed"
                    Data="M0 0 L2 0 L2 5 L0 5 z"
                    Fill="{TemplateBinding Foreground}"
                    UseLayoutRounding="True"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    Stretch="Fill"
                    Width="2"
                    Height="5"
                    SnapsToDevicePixels="True"/>
        </Grid>
    </ControlTemplate>
