    <ControlTemplate x:Key="GroupChromeTemplate" TargetType="telerikRibbonViewPrimitives:GroupChrome">
        <Grid Background="Transparent">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <Border Grid.RowSpan="2" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}"/>
            <Grid x:Name="Tray" Grid.Row="1" Margin="1 0 2 2">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                <ContentControl
                        Content="{TemplateBinding Header}"
                        ContentTemplate="{TemplateBinding HeaderTemplate}"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        IsTabStop="False"
                        Margin="3 0"
                        Foreground="{telerik:Windows8Resource ResourceKey=StrongBrush}"/>
                <telerikRibbonView:RadRibbonButton
                        MinHeight="0"
                        MinWidth="0"
                        Visibility="{TemplateBinding DialogLauncherVisibility}"
                        x:Name="DialogLauncher"
                        Grid.Column="1"
                        IsAutoSize="False"
                        TabIndex="1"
                        IsTabStop="False"
                        Command="{TemplateBinding DialogLauncherCommand}"
                        CommandParameter="{TemplateBinding DialogLauncherCommandParameter}"
                        CommandTarget="{TemplateBinding DialogLauncherCommandTarget}"
                        Margin="1"
                        Width="15"
                        Height="14">
                    <telerikRibbonView:RadRibbonButton.Content>
                        <Grid HorizontalAlignment="Center" VerticalAlignment="Center" Width="8" Height="8">
                            <Path Fill="{telerik:Windows8Resource ResourceKey=StrongBrush}" Data="M6,3 L7,3 L7,6 L7,7 L3,7 L3,6 L6,6 z M3,3 L4,3 L4,4 L6,4 L6,6 L4,6 L4,4 L3,4 z M0,0 L1,0 L6,0 L6,1 L1,1 L1,6 L0,6 L0,1 z"/>
                        </Grid>
                    </telerikRibbonView:RadRibbonButton.Content>
                </telerikRibbonView:RadRibbonButton>
            </Grid>
            <ContentPresenter Margin="{TemplateBinding Padding}"/>
        </Grid>
    </ControlTemplate>
    <Style x:Key="GroupChromeStyle" TargetType="telerikRibbonViewPrimitives:GroupChrome">
        <Setter Property="Template" Value="{StaticResource GroupChromeTemplate}"/>
        <Setter Property="Foreground" Value="{telerik:Windows8Resource ResourceKey=StrongBrush}"/>
        <Setter Property="Background" Value="{telerik:Windows8Resource ResourceKey=MainBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:Windows8Resource ResourceKey=BasicBrush}"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="BorderThickness" Value="0 0 1 0"/>
        <Setter Property="Padding" Value="3 3 5 3"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
