                                <DiscreteObjectKeyFrame KeyTime="00:00:00">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="BarTabStop">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="BarTab" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="00:00:00">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Path x:Name="Indent"
                    Stretch="Fill"
                    Data="M0 4 L4.5 0 9 4 9 8 L0 8 z"
                    Visibility="Collapsed"
                    RenderTransformOrigin="0.5 0.5"
                    Fill="{TemplateBinding Background}"
                    Height="8"
                    Width="9"
                    Stroke="{TemplateBinding Foreground}"/>
            <Path x:Name="FirstLineIndent"
                    Stretch="Fill"
                    RenderTransformOrigin="0.5 0.5"
                    Height="8"
                    Width="9"
                    Visibility="Collapsed"
                    Fill="{TemplateBinding Background}"
                    Stroke="{TemplateBinding Foreground}"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    Data="M0 4 L4.5 0 9 4 9 8 L0 8 z">
                <Path.RenderTransform>
                    <RotateTransform Angle="180"/>
                </Path.RenderTransform>
            </Path>
            <Rectangle x:Name="LeftIndent"
