                                        extensions:SettingsPaneView.EditorItemType="Connections"
                                        extensions:SettingsPaneView.EditorPropertyName="ConnectionType"
                                        extensions:SettingsPaneView.EditorValue="{Binding Path=(extensions:SettingsPaneView.RadioButtonValue), Mode=TwoWay, RelativeSource={RelativeSource Self}}">
