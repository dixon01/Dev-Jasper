                                    <StackPanel Grid.Column="1" Margin="10 0 0 0">
                                        <telerik:RadRadioButton GroupName="ConnectionType" Height="{StaticResource PaneGeometryButtonHeight}">
                                            <telerik:RadRadioButton.ContentTemplate>
                                                <DataTemplate>
                                                    <Path Fill="{Binding Foreground, RelativeSource={RelativeSource AncestorType=ContentControl}}" Stretch="None" Data="M5,10 C3.3,10 1.9,11.3 1.9,13 L1.9,18 L0,18 L2.0E-06,13 C-9.3E-07,10.2 2.2,8 4.9,8 M5.0,8 L23.0,8 L23.1,7.9 C24.7,7.9 26,6.6 26,5.0 L28,5.0 L27.9,5.1 C27.9,7.7 25.7,9.9 23.1,9.9 L23.0,9.9 L5.0,10 M26,4.9 L26,0 L28,0 L28,4.9"/>
                                                </DataTemplate>
                                            </telerik:RadRadioButton.ContentTemplate>
                                            <extensions:SettingsPaneView.RadioButtonValue>
                                                <core:ConnectionType>Spline</core:ConnectionType>
                                            </extensions:SettingsPaneView.RadioButtonValue>
                                        </telerik:RadRadioButton>
                                        <TextBlock Style="{StaticResource ButtonTextBlockStyle}" Text="{telerik:LocalizableResource Key=SettingsPane_RoundAngleConnectionType}"/>
                                    </StackPanel>
                                    <StackPanel Grid.Row="2" Margin="0 0 10 0">
                                        <telerik:RadRadioButton GroupName="ConnectionType" Height="{StaticResource PaneGeometryButtonHeight}">
                                            <telerik:RadRadioButton.ContentTemplate>
                                                <DataTemplate>
                                                    <Path Fill="{Binding Foreground, RelativeSource={RelativeSource AncestorType=ContentControl}}" Stretch="None" Data="M30.6,3.2 C30.6,3.2 23.5,15.3 16.2,4.9 C6.9,-8.3 0,9.4 0,9.4 L1.6,10.2 C1.6,10.2 7.2,-5.1 14.9,6.1 C23.0,18.2 32.0,4.2 32.0,4.2 z"/>
                                                </DataTemplate>
                                            </telerik:RadRadioButton.ContentTemplate>
                                            <extensions:SettingsPaneView.RadioButtonValue>
                                                <core:ConnectionType>Bezier</core:ConnectionType>
                                            </extensions:SettingsPaneView.RadioButtonValue>
