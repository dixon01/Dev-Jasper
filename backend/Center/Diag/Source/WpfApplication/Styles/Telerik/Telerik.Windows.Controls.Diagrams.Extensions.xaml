                                        <TextBlock Style="{StaticResource ButtonTextBlockStyle}" Text="{telerik:LocalizableResource Key=SettingsPane_BezierConnectionType}"/>
                                    </StackPanel>
                                </Grid>
                            </Grid>
                        </StackPanel>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style.Setters>
    </Style>
    <Style TargetType="extensions:SettingsPaneTextControl" x:Key="SettingsPaneTextControlStyle">
