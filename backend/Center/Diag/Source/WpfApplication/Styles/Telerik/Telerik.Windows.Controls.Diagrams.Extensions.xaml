                                                extensions:SettingsPaneView.EditorItemType="Connections"
                                                extensions:SettingsPaneView.EditorValue="{Binding Value, Mode=TwoWay, RelativeSource={RelativeSource Self}, Converter={StaticResource DoubleToSizeConverter}}"/>
                                        <telerik:RadNumericUpDown
