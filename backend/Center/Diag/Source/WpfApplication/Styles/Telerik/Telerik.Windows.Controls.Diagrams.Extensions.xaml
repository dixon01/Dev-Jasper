                                                Margin="5 0 0 0"
                                                Padding="{StaticResource SettingsPane_CapNumericsPadding}"
                                                extensions:SettingsPaneView.EditorPropertyName="TargetCapSize"
                                                extensions:SettingsPaneView.EditorItemType="Connections"
