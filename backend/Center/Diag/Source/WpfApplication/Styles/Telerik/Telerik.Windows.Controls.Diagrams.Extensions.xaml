                                        <ObjectAnimationUsingKeyFrames Duration="0:0:0" Storyboard.TargetName="Path" Storyboard.TargetProperty="Fill">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{telerik1:Windows8Resource ResourceKey=AccentBrush}"/>
