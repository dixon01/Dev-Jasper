                                Foreground="{Binding Foreground, RelativeSource={RelativeSource AncestorType=ContentControl}}"
                                Background="{StaticResource ButtonIconBackground_Normal}"
                                Template="{StaticResource ArrowTemplateCombobox}"/>
                    </DataTemplate>
                </telerik:RadToggleButton.ContentTemplate>
            </telerik:RadToggleButton>
            <Border x:Name="FocusBorder"
                    IsHitTestVisible="False"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    Visibility="Collapsed"
                    Background="Transparent"
                    Grid.ColumnSpan="2"
                    Opacity="0.2"
                    BorderBrush="{TemplateBinding BorderBrush}"/>
