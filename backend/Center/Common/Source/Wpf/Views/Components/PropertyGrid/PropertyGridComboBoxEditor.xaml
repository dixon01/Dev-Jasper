<propertyGrid:PropertyGridEditor x:Class="Gorba.Center.Common.Wpf.Views.Components.PropertyGrid.PropertyGridComboBoxEditor"
                             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                             xmlns:propertyGrid="clr-namespace:Gorba.Center.Common.Wpf.Views.Components.PropertyGrid"
                             mc:Ignorable="d"
                             x:Name="PARTS_PROPERTYGRIDCOMBOBOXEDITOR"
                             d:DataContext="{d:DesignInstance propertyGrid:PropertyGridItemDataSource}">
    <ComboBox SelectedItem="{Binding Value}" ItemsSource="{Binding DomainObject}"
              DisplayMemberPath="{Binding DisplayMemberPath, ElementName=PARTS_PROPERTYGRIDCOMBOBOXEDITOR}" IsReadOnly="True" IsEditable="True">
    </ComboBox>
</propertyGrid:PropertyGridEditor>