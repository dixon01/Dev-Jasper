            <Trigger Property="CarouselScrollButtonType" Value="PageRight">
                <Setter TargetName="ArrowForeground" Property="Data" Value="M5,0L6.0000002,0 6.0000002,1.0000001 7,1.0000001 7,2.0000001 8,2.0000001 8,3.0000001 9,3.0000001 9,4.0000001 8,4.0000001 8,5.0000001 7,5.0000001 7,6.0000001 6.0000002,6.0000001 6.0000002,7.0000001 5,7.0000001z M0,0L1,0 1,1.0000001 2,1.0000001 2,2.0000001 3,2.0000001 3,3.0000001 4,3.0000001 4,4.0000001 3,4.0000001 3,5.0000001 2,5.0000001 2,6.0000001 1,6.0000001 1,7.0000001 0,7.0000001z"/>
