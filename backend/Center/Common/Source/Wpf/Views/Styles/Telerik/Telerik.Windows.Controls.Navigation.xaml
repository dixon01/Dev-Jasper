                        <Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}"/>
                        <Border x:Name="MouseOverVisual" Opacity="0" Background="{telerik1:Windows8Resource ResourceKey=StrongBrush}"/>
                        <Border x:Name="SelectedVisual" Opacity="0" Background="{telerik1:Windows8Resource ResourceKey=BasicBrush}"/>
                        <Grid Margin="{TemplateBinding Padding}">
                            <telerik1:RadDockPanel>
                                <Image Source="{TemplateBinding Icon}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" telerik1:RadDockPanel.Dock="Left" Stretch="None"/>
                                <ContentControl x:Name="HeaderElement"
