                    <grid:FilteringDropDown x:Name="PART_DistinctFilterControl" Grid.Column="1" Visibility="{TemplateBinding FilteringUIVisibility}" Margin="0,0,-2,0"/>
                    <Thumb x:Name="PART_LeftHeaderGripper"
