                </Grid>
            </Border>
            <grid:FieldFilterControl x:Name="PART_FieldFilterControl" Visibility="{TemplateBinding FieldFilterControlVisibility}" Grid.Row="1"/>
        </Grid>
    </ControlTemplate>
