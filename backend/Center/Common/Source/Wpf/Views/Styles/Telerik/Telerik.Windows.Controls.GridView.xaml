                                Width="12"
                                Height="10"
                                Visibility="Collapsed"
                                Background="{telerik:Windows8Resource ResourceKey=MainBrush}"
                                BorderBrush="{telerik:Windows8Resource ResourceKey=StrongBrush}"
                                BorderThickness="1">
                            <Path
                                    Fill="{telerik:Windows8Resource ResourceKey=StrongBrush}"
                                    Stretch="Fill"
                                    Data="M3,2 L4,2 L5,2 L6,2 L6,3 L5,3 L5,4 L5,5 L5,6 L5,7 L6,7 L6,8 L5,8 L4,8 L3,8 L3,7 L4,7 L4,6 L4,5 L4,4 L4,3 L3,3 z"
