                </MultiTrigger.Conditions>
                <Setter TargetName="SelectionBackground" Property="BorderBrush" Value="{telerik:Windows8Resource ResourceKey=ValidationBrush}"/>
                <Setter TargetName="SelectionBackground" Property="BorderThickness" Value="0,1,1,1"/>
                <Setter TargetName="InvalidEditableIndicator" Property="Visibility" Value="Visible"/>
