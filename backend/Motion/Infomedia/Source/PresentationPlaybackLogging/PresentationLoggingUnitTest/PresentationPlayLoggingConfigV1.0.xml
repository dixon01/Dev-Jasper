    <FileNameRolloverType>Numerical</FileNameRolloverType>
    <MaxFileSize>0</MaxFileSize>
    <MaxRecords>5000</MaxRecords>
    <MaxZipFileEntries>0</MaxZipFileEntries>
    <RollOverLogOutputFolder>D:\PresentationPlayLogs</RollOverLogOutputFolder>
