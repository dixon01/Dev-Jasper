<?xml version="1.0" encoding="utf-8"?>
<Dictionary
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="file:///. dictionary.xsd">
  <Version>2.0</Version>
  <Languages>
    <Language Index="0" Name="en" />
  </Languages>
  <Tables>
    <Table Index="0" Name="SystemStatus">
      <Description>Table containing all system information.</Description>
      <Column Index="0" Name="Date" />
      <Column Index="1" Name="Time" />
      <Column Index="2" Name="RemotePC" />
      <Column Index="3" Name="StopRequestState"/>
      <Column Index="4" Name="DoorStatus"/>
    </Table>
    <Table Index="1" Name="PagingStatus">
      <Description>
        Table containing the page number and number of pages used in paging, these 
        values are generated by the Composer, not by Protran.
        Don't change these values since they are hardcoded in the Composer!
      </Description>
      <Column Index="0" Name="NumberOfPages" />
      <Column Index="1" Name="PageNumber" />
    </Table>
    <Table Index="10" Name="Route">
      <Description>Information about the current route</Description>
      <Column Index="0" Name="Line"/>
      <Column Index="1" Name="SpecialLine"/>
      <Column Index="2" Name="Region"/>
      <Column Index="3" Name="Punctuality"/>
    </Table>
    <Table Index="11" Name="Destination">
      <Description>Name of the destination stop</Description>
      <Column Index="0" Name="DestinationName"/>
      <Column Index="1" Name="DestinationCity"/>
      <Column Index="2" Name="DestinationInfo"/>
      <Column Index="3" Name="DestinationTime"/>
      <Column Index="4" Name="DestinationDelay"/>
      <Column Index="5" Name="DestinationTransferSymbols"/>
    </Table>
    <Table Index="12" Name="Stops">
      <Description>Table containing all stop information.</Description>
      <Column Index="0" Name="StopName" />
      <Column Index="1" Name="StopCity" />
      <Column Index="2" Name="StopInfo"/>
      <Column Index="3" Name="StopTime"/>
      <Column Index="4" Name="StopDelay"/>
      <Column Index="5" Name="StopTransferSymbols"/>
    </Table>
    <Table Index="13" Name="Connections">
      <Description></Description>
      <Column Index="0" Name="ConnectionTransportType"/>
      <Column Index="1" Name="ConnectionLineNumber"/>
      <Column Index="2" Name="ConnectionLineName"/>
      <Column Index="3" Name="ConnectionTime"/>
      <Column Index="4" Name="ConnectionDestinationName"/>
      <Column Index="5" Name="ConnectionDestinationCity"/>
      <Column Index="6" Name="ConnectionVia"/>
      <Column Index="7" Name="ConnectionPlatform"/>
      <Column Index="8" Name="ConnectionInfo"/>
      <Column Index="9" Name="ConnectionDelay"/>
    </Table>
    <Table Index="20" Name="PassengerMessages">
      <Description></Description>
      <Column Index="0" Name="MessageType"/>
      <Column Index="1" Name="MessageTitle"/>
      <Column Index="2" Name="MessageText"/>
    </Table>
  </Tables>
</Dictionary>